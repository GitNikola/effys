			<urllinks>
			<$authorurl=$ret>
			<urltext>
			<$author=$ret>
			<trimmer inout=$&author>
		</if>
		<if $SITEDOMAIN eq 'forum.mesec.cz' or $SITEDOMAIN eq 'forums.poz.com' or $SITEDOMAIN eq 'www.oknamedia.ru'
			or $SITEDOMAIN eq 'betonbasket.ru' or $SITEDOMAIN eq 'www.mamask.ru' or $SITEDOMAIN eq 'www.birtydastards.com'>
			<rex max=1 '>><div class\="=floatleft ?poster"=!<h4>+<h4>\P=!</h4>+' $postdata></rex>
