		<elseif $SITEDOMAIN eq 'www.foroche.com' or $SITEDOMAIN eq 'www.captiva-club.com'>
			<rex '>><div class\="topbar_post"=!</div>+' $postdata>
		<elseif $SITEDOMAIN eq 'forum.vatan.tc' or $SITEDOMAIN eq 'ilgiliforum.com'>
			<rex '>><div class\="date_post"=!</div>+' $postdata>
		<elseif $SITEDOMAIN eq 'rexik.zoznam.sk'>
			<rex max=1 '<span=!<....+>>class\="message-date"=!</span>+</span>=' $postdata></rex>
		</if>
		<if $SITEDOMAIN eq 'www.svetecigaret.cz'>
			<sandr '//' ' ' $ret>
		<elseif $SITEDOMAIN eq 'www.thaidellclub.com'>
			<sandr '>><strong=!</strong>+' ' ' $ret>
			<sandr ' à¹€à¸§à¸¥à¸² ' ' ' $ret>
			<sandr 'à¸§à¸±à¸™à¸™à¸µà¹‰' 'today' $ret>
		</if>
		<fetch localhost/index.html $ret>
		<urltext>
		<sandr ' del ' ' ' $ret>
		<sandr ' um ' ' ' $ret>
		<sandr ' v ' ' ' $ret>
		<sandr ' alle ' ' ' $ret>
		<sandr ' om ' ' ' $ret>
		<if $LANG ne 'Thai'>
			<sandr '\. ' '.' $ret>
		</if>
		<sandr '.*>>Gepost op:' '' $ret>
		<!-- this character appears in some Russian dates and will break the translations
			it needs a space inserted somewhere to make it 'safer' before it can be uncommented
		<sandr '»' '' $ret>-->
		<sandr ' Ð² ' ' ' $ret>
		<sandr ' â ' ' ' $ret>		
		<$lastpostdate=$ret>
		<trimmer inout=$&lastpostdate>
		<if $LANG eq "Spanish">
			<sandr '>> a las =' ' ' $lastpostdate>
			<$lastpostdate=$ret>
		<elseif $LANG eq 'Czech'>
			<if $SITEDOMAIN eq 'forum.podnikatel.cz' or $SITEDOMAIN eq 'forum.mesec.cz'>
				<rex '>>kdy:\P=.+' $lastpostdate>
				<$lastpostdate=$ret>
			</if>
		<elseif $LANG eq "French">
			<sandr ' Ã ' ' ' $lastpostdate>
			<if $SITEDOMAIN eq 'forum.freenews.fr'>
				<sandr '>>=! le: * le: =' ' ' $ret>
			</if>
			<$lastpostdate=$ret>
		<elseif $LANG eq "Polish">
			<sandr  ' o ' ' ' $ret>
			<if $SITEDOMAIN eq 'brzytwa.org' or $SITEDOMAIN eq 'www.forum.e-mebel.net'>
				<sandr '>>=! dnia: * dnia: =' ' ' $ret>
			</if>
		<elseif $LANG eq 'Russian'>
			<if $SITEDOMAIN eq 'forza-club.org.ua'>
				<sandr ' â ' ' ' $ret>
			</if>
		</if>
			<$lastpostdate=$ret>
		<if $SITEDOMAIN eq 'forza-club.org.ua' or $SITEDOMAIN eq 'www.toyota-club.com.ua'
			or $SITEDOMAIN eq 'forum.alternatifim.com' or $SITEDOMAIN eq 'betonbasket.ru'
			or $SITEDOMAIN eq 'kvartiradin.ru'>
			<sandr '.+>>#=\digit+ :=\F.+' '' $ret>
			<$lastpostdate=$ret>
		</if>
		<if $SITEDOMAIN eq 'rexik.zoznam.sk'>
			<sandr '(' '' $lastpostdate>
			<sandr ')' '' $ret>
			<$lastpostdate = $ret>
			<fixdate2 inoutdate=$&lastpostdate dateformat=$TIMEFORMATID>
		</if>
		<if $SITEDOMAIN eq 'www.prado-club.su'>
			<sandr '\digit\P=\space=\F\digit=' '.' $lastpostdate>
			<$lastpostdate = $ret>
		</if>
		<if $SITEDOMAIN eq 'rebenok.by'>
			<rex max=1 '>><div class\="post"=.+' $postdata></rex>
			<rex max=1 '>><span=!</span>+' $ret></rex>
			<fetch localhost/index.html $ret>
			<urltext>
			<$lastpostdate=$ret>
			<!-- convert dd:mm:yyyy hh:mm to dd.mm.yyyy hh:mm -->
			<sandr '>>\digit\digit\P=:=\F\digit\digit:\digit\digit\digit\digit=' '.' $lastpostdate>
			<sandr '>>\digit\digit\.\digit\digit\P=:=\F\digit\digit\digit\digit=' '.' $ret>
			<$lastpostdate=$ret>
		</if>
		<local temp>
		<if $SITEDOMAIN eq 'www.nuggetoutdoors.com.au' or $SITEDOMAIN eq 'forum.b-m-w.ru'>
			<rex max=1 '>>\digit\digit:\digit\digit=:\digit\digit?' $lastpostdate></rex>
			<$temp=$ret>
			<sandr '>>\digit\digit:\digit\digit=:\digit\digit?' '' $lastpostdate>
			<sandr '!\digit+>>=' ' ' $ret>
			<sum '%s' $ret $temp>
			<$lastpostdate=$ret>
		</if>

		<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
		<!-- GENDER -->
		<rex 'images/\P=fe?male=\F\.gif"=' $postdata>
		<lower $ret>
		<if $ret eq 'female'>
			<$gender="F">
		<elseif $ret eq 'male'>
			<$gender="M">
		</if>
		<if $SITEDOMAIN eq 'rexik.zoznam.sk'>
			<rex max=1 '<img=!<...+>>class\="gender"=!>+>=' $postdata></rex>
			<rex max=1 '>>alt\="\P=!"+' $ret></rex>
			<if $ret eq 'girl'>
				<$gender="F">
			<elseif $ret eq 'boy'>
				<$gender="M">
			</if>

		</if>
		<!-- LOCATION -->
		<$locrex= '>>Ubicacion: \P=!<br+' '>>>Regione:\P=!<+' '>>>Miejsce pobytu:\P=!<+' '>>>location:\P=!<+'
			'>>>Ð“Ð¾Ñ€Ð¾Ð´:\P=!<+' '>>>ÐÐ´Ñ€ÐµÑ:\P=!<+' '>>>Ð ÐµÐ³Ð¸Ð¾Ð½:\P=!<+' '>>>Îòêóäà:\P=!<+'
			'>>>Ð“Ð¾Ñ€Ð¾Ð´/Ð Ð°Ð¹Ð¾Ð½:\P=!<+' '>>Ð¡Ñ‚Ñ€Ð°Ð½Ð°:\P=!<img+' '>>>Region:\P=!<+' '>>Land:=!title+title\="\P=!"+'
			'<li=!<..+>>class\="location=!</li>+</li>=' '<li=!<..+>>Location :\P=!</li>+</li>='>
		<rex $locrex $postdata>
		<fetch localhost/index.html $ret>
		<urltext>
		<$authorloc=$ret>
		<trimmer inout=$&authorloc>
		<if $authorloc eq ''>
			<!-- take text from image -->
			<$locrex='>>>Country:=!>+>=' '>>>Ð¡Ñ‚Ñ€Ð°Ð½Ð°:=!>+>='>
			<rex $locrex $postdata>
			<rex max=1 '>>title\="\P=!"+' $ret></rex>
			<fetch localhost/index.html $ret>
			<urltext>
			<$authorloc=$ret>
			<trimmer inout=$&authorloc>
		</if>
		<!-- AGE -->
		<$agerex='>>>Age:\P=!<+'>
		<rex $agerex $postdata>
		<fetch localhost/index.html $ret>
		<urltext>
		<$authorage=$ret>
		<trimmer inout=$&authorage>
		<rex '\digit+' $authorage>
		<$authorage=$ret>
		<!-- SIGNATURE -->
		<rex '>><div class\="signature"=.+' $postdata>
		<getdivs data=$ret divs=$&divs>
		<loop max=1 $divs>
			<rex ">\P=.+" $divs>
			<$signature=$ret>
			<add_cleanup inouthtml=$&signature>
			<trimmer inout=$&signature>
		</loop>
		<if $REGISTRATIONDATEFORMATID ne '' AND $REGISTRATIONDATEFORMATID ne '0'>
			<$authorregrex='>>Ñ íàìè ñ:</span>\P=!<+' '>>>Ð—Ð°Ñ€ÐµÐ³Ð¸ÑÑ‚Ñ€Ð¸Ñ€Ð¾Ð²Ð°Ð½:\P=!<+' '>>reg:\P=!<li+'>
			<rex $authorregrex $postdata>
			<if $SITEDOMAIN eq 'gelandewagen.org'>
				<sandr '\.' '' $ret>
			</if>
			<fetch localhost/index.html $ret>
			<urltext>
			<$registered=$ret>
			<trimmer inout=$&registered>
			<fixregdate passdate=$&registered>
			<checkdate lang=$LANG isdateonly=1 indate=$&registered indateid=$REGISTRATIONDATEFORMATID registereddate=1>
		</if>
		<!-- SITEPOSTID - using anchor value by default -->
		<loop max=1 $anchor><$sitepostid=$anchor></loop>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
		<if ($SITEDOMAIN eq 'dodge-club.org.ua' or $SITEDOMAIN eq 'www.corsa-club.net' )
		and $rexloop eq 1 and $FIRSTPAGE neq 1>
			SKIP TS
			<continue>
		</if>
		<!-- CHARSET CONVERSION -->
                <if $CHARSETSRC ne '' and $CHARSET ne '' and $CHARSETSRC ne $CHARSET>
			<urlutil charsetconv $htmlbody $CHARSETSRC $CHARSET>
			<$htmlbody=$ret>
			<urlutil charsetconv $signature $CHARSETSRC $CHARSET>
			<$signature=$ret>
                </if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $HASUNIQUESITEPOSTID eq 1 and $sitepostid eq '' >
INVALID POST - SITEPOSTID REQUIRED
		<elseif $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $FIRSTPAGE eq 1 and $rexloop eq 0>
				<$threadstarter=1>
			<else>
				<$threadstarter=0>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$postbody
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=11>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>
<a name=parsemethod12 private textdata>
method12
	<domain_blocker callingmethod='12' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>
	<rex row '<a name=!<a name+<table=!<table+>>userinfo\.php=!userinfo\.php+' $textdata>
		<if $next eq 1 and ($SITEDOMAIN eq 'forum.tecnozoom.it' or $SITEDOMAIN eq 'forum.tuttogratis.it')>
			<!-- first post doesnt have an anchor before the table which messes things us -->
			<rex '<table=!<table+>>userinfo\.php=.+' $ret>
			<$isfirstpost=1>
		</if>
		<$postdata=$ret>
		<$rexloop=$loop>
		<clearpostrecbuffer>
		<if $isfirstpost eq 0>
			<!-- use this flag to prevent anchor parsing of first post, since it does not have one and will grab the wrong one -->
			<getanchor data=$postdata>
		</if>
		<$isfirstpost=0>
		<$posttable=>
		<gettables data=$postdata tables=$&tables>
		<loop max=1 $tables>
			<$posttable=$tables>
		</loop>
		<getrows data=$posttable rows=$&rows>
		<loop max=2 $rows>
			<if $next eq 1>
				<$postrow1=$rows>
			<elseif $next eq 2>
				<$postrow2=$rows>
			</if>
		</loop>
		<getcells data=$postrow1 cells=$&cells>
		<loop $cells>
			<$cellnext=$next>
			<rex ">\P=.+" $cells>
			<$coldata=$ret>
			<if $cellnext eq 1>
				<rex max=1 '<a =!<a +>>userinfo.php=!</a>+</a>=' $coldata></rex>
				<fetch $THREADURL $ret>
				<urllinks>
				<$authorurl=$ret>
				<urltext>
				<$author=$ret>
				<trimmer inout=$&author>
			<elseif $cellnext eq 3>
				<fetch localhost/index.html $coldata>
				<urltext>
				<sandr 'Inviato il' '' $ret>
				<$lastpostdate=$ret>
				<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
			</if>
		</loop>
		<getcells data=$postrow2 cells=$&cells>
		<loop $cells>
			<$cellnext=$next>
			<rex ">\P=.+" $cells>
			<$coldata=$ret>
			<if $cellnext eq 1>
				<$avatarrex='<img=!<img+>>class\="avatar"=!>+>=' >
				<rex max=1 $avatarrex $coldata></rex>
				<getimagesrc htmlsrc=$ret domain=$THREADURL srcret=$&avatarurl>
			<elseif $cellnext eq 2>
				<getdivs data=$coldata divs=$&bodydiv>
				<loop max=1 $bodydiv>
					<gettagtextorhtml tag=$bodydiv istext=0 textret=$&htmlbody>
					<gettagtextorhtml tag=$bodydiv istext=1 textret=$&postbody>
				</loop>
				<rex '>><div class\="signature=.+' $coldata>
				<getdivs data=$ret divs=$&sigdiv>
				<loop max=1 $sigdiv>
					<gettagtextorhtml tag=$sigdiv istext=0 textret=$&signature>
				</loop>
				<trimmer inout=$&signature>
			</if>
		</loop>
		<if $SITEDOMAIN eq 'forum.tuttogratis.it'>
			<rex max=1 '>>id\="\P=modified_=\digit+' $postdata></rex>
			<$anchor=$ret>
		</if>
		<!-- SITEPOSTID - using anchor value by default -->
		<loop max=1 $anchor><$sitepostid=$anchor></loop>
		<if $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $HASUNIQUESITEPOSTID eq 1 and $sitepostid eq '' >
INVALID POST - SITEPOSTID REQUIRED
		<elseif $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $FIRSTPAGE eq 1 and $rexloop eq 0>
				<$threadstarter=1>
			<else>
				<$threadstarter=0>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$bodynoquotes
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=12>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>

<a name=parsemethod13 private textdata>
method13
	<domain_blocker callingmethod='13' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>
	<rex row '>><a\spacename\="msg=!<a\spacename\="msg+' $textdata>
		<$postdata=$ret>
		<$rexloop=$loop>
		<clearpostrecbuffer>
		<$postheader=>
		<$postcontent=>
		<$sigcontent=>
		<getanchor data=$postdata>
		<$posttable=>
		<rex max=1 '>><\!\-\-\-/ Start Header=.+' $postdata></rex>
		<getdivs data=$ret divs=$&divs>
		<loop max=4 $divs>
			<if $next eq 1>
				<$datediv=$divs>
			<elseif $next eq 2>
				<$headerdiv=$divs>
			<elseif $next eq 3>
				<$titlediv=$divs>
			<elseif $next eq 4>
				<$contentdiv=$divs>
			</if>
		</loop>
		<rex row '<a\space=!<a\space+>>action\=profile=!</a>+</a>' $headerdiv>
			<$linkdata=$ret>
			<fetch $THREADURL $linkdata>
			<urltext>
			<$author=$ret>
			<trimmer inout=$&author>
			<if $author ne "">
				<urllinks>
				<loop max=1 $ret>
					<$authorurl=$ret>
				</loop>
				<break>
			</if>
		</rex>
		<if $author eq ''>
			<rex max=1 '>><h4>\P=!</h4>+' $headerdiv></rex>
			<$author=$ret><!-- no active link - no author url -->
		</if>
		<rex max=1 '>>/Female\.gif"' $headerdiv></rex>
		<if $ret eq ''>
			<rex max=1 '>>/Male\.gif"' $headerdiv></rex>
			<if $ret neq ''>
				<$gender='M'>
			</if>
		<else>
			<$gender='F'>
		</if>
		<rex '>><div id\="subject_=!</div>+' $titlediv>
		<fetch localhost/index.html $ret>
		<urltext>
		<$posttitle=$ret>
		<trimmer inout=$&posttitle>
		<rex '>\P=.+' $datediv>
		<fetch localhost/index.html $ret>
		<urltext>
		<sandr '>>Reply \#=\digit+' '' $ret>
		<$lastpostdate=$ret>
		<trimmer inout=$&lastpostdate>
		<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
		<getdivs data=$contentdiv divs=$&divs>
		<loop max=2 $divs>
			<if $next eq 1>
				<gettagtextorhtml tag=$divs istext=0 textret=$&htmlbody>
				<gettagtextorhtml tag=$divs istext=1 textret=$&postbody>
				<removequotes bodydata=$postbody bodynoquotes=$&bodynoquotes>
			<elseif $next eq 2>
				<rex '>><div class\="signature"=.+' $divs>
				<getdivs data=$ret divs=$&sigdiv>
				<loop max=1 $sigdiv><rex '>\P=.+' $sigdiv><$signature=$ret></loop>
				<trimmer inout=$&signature>
			</if>
		</loop>
		<!-- AVATAR -->
		<$avatarrex='<img=!<img+>>class\="avatar"=!>+>=' >
		<rex max=1 $avatarrex $postdata></rex>
		<getimagesrc htmlsrc=$ret domain=$THREADURL srcret=$&avatarurl>
		<!-- SITEPOSTID - using anchor value by default -->
		<loop max=1 $anchor><$sitepostid=$anchor></loop>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $HASUNIQUESITEPOSTID eq 1 and $sitepostid eq '' >
INVALID POST - SITEPOSTID REQUIRED
		<elseif $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $FIRSTPAGE eq 1 and $rexloop eq 0>
				<$threadstarter=1>
			<else>
				<$threadstarter=0>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$bodynoquotes
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=13>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>

<a name=parsemethod14 private textdata>
method14
	<domain_blocker callingmethod='14' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>
	<rex row '>><\!\-\- Post \digit=!<\!\-\- End Post \digit+' $textdata>
		<$postdata=$ret>
		<$rexloop=$loop>
		<clearpostrecbuffer>
		<$datecell=>
		<$authorcell=>
		<$bodycell=>
		<getcells data=$postdata cells=$&cells>
		<loop max=3 $cells>
			<if $next eq 1>
				<$datecell=$cells>
			<elseif $next eq 2>
				<$authorcell=$cells>
			<elseif $next eq 3>
				<$bodycell=$cells>
			</if>
		</loop>
		<!-- ANCHOR -->
		<rex max=1 '>>#\P=msg=\digit+' $datecell></rex>
		<$anchor=$ret>
		<!-- AUTHOR -->
		<rex max=1 '>><b>\P=!</b>+' $authorcell></rex>
		<fetch localhost/index.html $ret>
		<urltext>
		<$author=$ret>
		<trimmer inout=$&author>
		<!-- TITLE -->
		<rex '<div=!<div+>>id\="subject_=!</div>+' $bodycell>
		<fetch localhost/index.html $ret>
		<urltext>
		<$posttitle=$ret>
		<trimmer inout=$&posttitle>
		<!-- DATE -->
		<rex '<td=!<td+>>id\="top_subject=!</td>+' $datecell>
		<fetch localhost/index.html $ret>
		<urltext>
		<$lastpostdate=$ret>
		<trimmer inout=$&lastpostdate>
		<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
		<!-- BODY & SIG -->
		<rex max=1 '>><div class\="post=.+' $bodycell></rex>
		<getdivs data=$ret divs=$&divs>
		<loop max=1 $divs><rex '>\P=.+' $divs><$htmlbody=$ret></loop>
		<fetch localhost/index.html $htmlbody>
		<urltext>
		<$postbody=$ret>
		<trimmer inout=$&postbody>
		<rex max=1 '>><div class\="signature=.+' $bodycell></rex>
		<getdivs data=$ret divs=$&divs>
		<loop max=1 $divs><rex '>\P=.+' $divs><$signature=$ret></loop>
		<!-- AVATAR -->
		<$avatarrex='<img=!<img+>>class\="avatar"=!>+>=' >
		<rex max=1 $avatarrex $authorcell></rex>
		<getimagesrc htmlsrc=$ret domain=$THREADURL srcret=$&avatarurl>
		<!-- SITEPOSTID - using anchor value by default -->
		<loop max=1 $anchor><$sitepostid=$anchor></loop>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $HASUNIQUESITEPOSTID eq 1 and $sitepostid eq '' >
INVALID POST - SITEPOSTID REQUIRED
		<elseif $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $FIRSTPAGE eq 1 and $rexloop eq 0>
				<$threadstarter=1>
			<else>
				<$threadstarter=0>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$bodynoquotes
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=14>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>

<a name=parsemethod15 private textdata>
method15
	<domain_blocker callingmethod='15' myreturn=$&stopproc>
	<local temp>
	<if $stopproc eq 1>
		<return>
	</if>
	<$rexloop=0>
	<$rexpattern='<table=!<table+>>class\="webboard_read_post[12]=">=!class\="webboard_read_post[12]+'>
	<rex row $rexpattern $textdata>
		<$postdata=$ret>
		<$rexloop=$loop>
		<sum "%s" $postdata '</tr>'>
		<$postdata=$ret>
		<clearpostrecbuffer>
		<$postrow1=>
		<getrows data=$postdata rows=$&rows>
		<loop max=1 $rows>
			<$postrow1=$rows>
		</loop>
		<getcells data=$postrow1 cells=$&cells>
		<loop $cells>
			<$cellnext=$next>
			<if $cellnext eq 1>
				<rex row skip=1 max=1 '<a\space=!<a\space+>>action\=profile=!</a>+</a>' $cells></rex>
				<rex row max=1 '>><a\space=!</a>+</a>=' $ret></rex>
				<fetch $THREADURL $ret>
				<urltext>
				<$author=$ret>
				<trimmer inout=$&author>
				<if $author ne "">
					<urllinks>
					<$authorurl=$ret>
				<else>
					<rex '>><div\spaceclass\="smalltext">\P=!<div\space+' $cells>
					<$author=$ret>
					<trimmer inout=$&author>
				</if>
				<!-- AVATAR -->
				<$avatarrex='<img=!<img+>>avatartype\==!>+>='>
				<rex max=1 '<a\space=!<a\space+>>action\=profile=!</a>+</a>' $cells></rex>
				<rex max=1 $avatarrex $ret></rex>
				<getimagesrc htmlsrc=$ret domain=$THREADURL srcret=$&avatarurl>
				<!--<rex '>>imgurl\=\P=.+' $avatarurl>-->
				<$avatarurl=$ret>
			<elseif $cellnext eq 2>
				<!-- SITEPOSTID -->
				<rex max=1 '>><div\spaceclass\="tools\-post">=!</div>+' $cells></rex>
				<rex '>>action\=post;quote\=\P=!;topic\=+' $ret>
				<$sitepostid=$ret>
				<!-- ANCHOR -->
				<sum '%s' 'msg' $sitepostid>
				<$anchor=$ret>
				<!-- POST TITLE -->
				<if $FIRSTPAGE eq 1 and $rexloop eq 0>
					<rex '>><h2=!</h2>+</h2>=' $cells>
				<else>
					<rex '<strong\space=!<strong\space+>>\F<div\spaceclass\="date\-post">=' $cells>
				</if>
				<fetch localhost/index.html $ret>
				<urltext>
				<$posttitle=$ret>
				<!-- POST DATE -->
				<rex '>><span\spaceclass\="topic\-rate=!</div>+' $cells>
				<sandr '<div\spaceid\="google\-ads">=.+' '' $ret>
				<sandr '<style\spacetype\="text/css">=.+' '' $ret>
				<rex "!</span>+>>=" $ret>
				<sandr '<b>' '' $ret>
				<sandr '</b>' '' $ret>
				<$lastpostdate=$ret>
				<trimmer inout=$&lastpostdate>
				<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
				<!-- POST BODY & HTML -->
				<rex '<div=!<div+>>class\="post">=.+' $cells>
				<getelements name='div' data=$ret elems=$&temp>
				<fix_multivar inout=$&temp>
				<$bodydiv=$temp>
				<loop max=1 $bodydiv>
					<gettagtextorhtml tag=$bodydiv istext=0 textret=$&htmlbody>
					<gettagtextorhtml tag=$bodydiv istext=1 textret=$&postbody>
				</loop>
			</if>
		</loop>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
		<if $invalidfound eq 0>
			<$rexloop=($rexloop+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $HASUNIQUESITEPOSTID eq 1 and $sitepostid eq '' >
INVALID POST - SITEPOSTID REQUIRED
		<elseif $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $FIRSTPAGE eq 1 and $rexloop eq 1>
				<$threadstarter=1>
			<else>
				<$threadstarter=0>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$bodynoquotes
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=15>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>

<a name=parsemethod16 private textdata>
method16
	<domain_blocker callingmethod='16' myreturn=$&stopproc>
	<local temp tdelements authordata avatarrex>
	<if $stopproc eq 1>
		<return>
	</if>
	<$rexloop=0>
	<rex '>>id\="quickModForm"=!<table\P+<table=.+' $textdata>
	<getelements name='table' data=$ret elems=$&temp>
	<fix_multivar inout=$&temp>
	<getelements name='td' data=$ret elems=$&tdelements>
	<loop $tdelements>
		<$postdata=$tdelements>
		<$rexloop=$loop>
		<$authordata=>
		<clearpostrecbuffer>
		<rex '<iframe=!<iframe+</iframe>=!</iframe>\F+>><img\spaceid\="my_add_button=' $postdata>
		<if $ret neq ''>
			<sandr '<iframe' '<a' $ret>
			<sandr '</iframe' '</a' $ret>
			<sandr 'src\=' 'href\=' $ret>
			<fetch $THREADURL $ret>
			<sleep 1 wake>
			<urllinks>
			<fetch $ret>
			<$authordata=$ret>
			<rex max=1 '>>id\="hpNickNameShort"=!<a\space\P+<a\space=!</a>+</a>=' $authordata></rex>
			<fetch $THREADURL $ret>
			<urltext>
			<$author=$ret>
			<trimmer inout=$&author>
			<urllinks>
			<$authorurl=$ret>
		<else>
			<rex '<td=!<td+>>/avatar_guest\.jpg=!</td>+' $postdata>
			<rex '>><div\spaceclass\="smalltext">\P=!</div>+' $ret>
			<fetch localhost/index.html $ret>
			<urltext>
			<$author=$ret>
			<trimmer inout=$&author>
		</if>
		<if $author eq ''>
			<rex row skip=1 max=1 '<a\space=!<a\space+>>action\=profile=!</a>+</a>' $postdata></rex>
			<rex row max=1 '>><a\space=!</a>+</a>=' $ret></rex>
			<fetch $THREADURL $ret>
			<urltext>
			<$author=$ret>
			<trimmer inout=$&author>
			<if $author ne "">
				<urllinks>
				<$authorurl=$ret>
			<else>
				<rex '>><div\spaceclass\="smalltext">\P=!<div\space+' $postdata>
				<$author=$ret>
				<trimmer inout=$&author>
			</if>
		</if>
		<!-- AVATAR -->
		<$avatarrex=
					'>><img\space=!/>+/>='
					'<img=!<img+>>avatartype\==!>+>='
		>
		<if $authordata neq ''>
			<rex max=1 '>>id\="lblSmallAvatar"=!</span>+' $authordata></rex>
		<else>
			<rex max=1 '<a\space=!<a\space+>>action\=profile=!</a>+</a>' $postdata></rex>
		</if>
		<rex max=1 $avatarrex $ret></rex>
		<getimagesrc htmlsrc=$ret domain=$THREADURL srcret=$&avatarurl>
		<$avatarurl=$ret>
		<!-- SITEPOSTID -->
		<rex max=1 '>>id\="subject_\P=\digit+\F"=' $postdata></rex>
		<$sitepostid=$ret>
		<!-- ANCHOR -->
		<sum '%s' 'msg' $sitepostid>
		<$anchor=$ret>
		<!-- POST TITLE -->
		<rex max=1 '<div\space=!<div\space+>>id\="subject_=!</div>+</div>=' $postdata></rex>
		<fetch localhost/index.html $ret>
		<urltext>
		<$posttitle=$ret>
		<!-- POST DATE -->
		<rex max=1 '<td=!<td+>>id\="subject_=.+' $postdata></rex>
		<getelements name='td' data=$ret elems=$&temp>
		<fix_multivar inout=$&temp>
		<rex '<b>=!>*>>à¹€à¸¡à¸·à¹ˆà¸­:</b>\P=!&#187;+' $temp>
		<sandr '<b>' '' $ret>
		<sandr '</b>' '' $ret>
		<$lastpostdate=$ret>
		<trimmer inout=$&lastpostdate>
		<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
		<!-- SIGNATURE -->
		<rex max=1 '<td=!<td+>>class\="smalltext giveIMG"=.+' $postdata></rex>
		<getelements name='td' data=$ret elems=$&temp>
		<fix_multivar inout=$&temp>
		<rex '<hr=!<hr+>>class\="hrcolor"=.+' $temp>
		<$signature=$ret>
		<!-- POST BODY & HTML -->
		<rex '<div=!<div+>>class\="post giveIMG">=.+' $postdata>
		<getelements name='div' data=$ret elems=$&temp>
		<fix_multivar inout=$&temp>
		<$bodydiv=$temp>
		<loop max=1 $bodydiv>
			<gettagtextorhtml tag=$bodydiv istext=0 textret=$&htmlbody>
			<gettagtextorhtml tag=$bodydiv istext=1 textret=$&postbody>
		</loop>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
		<if $invalidfound eq 0>
			<$rexloop=($rexloop+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $HASUNIQUESITEPOSTID eq 1 and $sitepostid eq '' >
INVALID POST - SITEPOSTID REQUIRED
		<elseif $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $FIRSTPAGE eq 1 and $rexloop eq 1>
				<$threadstarter=1>
			<else>
				<$threadstarter=0>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$bodynoquotes
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=16>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</loop>
</a>

<a name=parsemethod17 private textdata>
method17
	<local mainrex postdata rexloop bodydiv daterex>
	<domain_blocker callingmethod='17' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>
	<$mainrex='<div\space=!<div\space*>>class\="message"=!class\="message"+'>
	<rex row $mainrex $textdata>
		<$rexloop=$loop>
		<getelements data=$ret name='div' elems=$&postdata max=1>
		<clearpostrecbuffer>
		<!-- AUTHOR & URL-->
		<rex max=1 '<a =!<a +>>action\=profile=!</a>+</a>=' $postdata></rex>
		<fetch $THREADURL $ret>
		<urllinks>
		<$authorurl=$ret>
		<urltext>
		<$author=$ret>
		<trimmer inout=$&author>
		<if $author eq ''>
			<rex max=1 '<div=!<...+>>class\="message-author=!</div>+</div>=' $postdata></rex>
			<sandr '"">' '">' $ret>
			<fetch localhost/index.html $ret>
			<urltext>
			<$author=$ret>
			<trimmer inout=$&author>
		</if>
		<!-- TITLE -->
		<rex '<a=!<a*>>class\="themelink"=!</a>+</a>=' $postdata>
		<fetch $THREADURL $ret>
		<urltext>
		<$posttitle=$ret>
		<trimmer inout=$&posttitle>
		<!-- ANCHOR -->
		<urllinks>
		<rex max=1 '>>#\P=.+' $ret></rex>
		<$anchor=$ret>
		<!-- BODY -->
		<rex '>><div\spaceclass\="msg"=.+' $postdata>
		<getdivs data=$ret divs=$&bodydiv>
		<loop max=1 $bodydiv>
			<sandr '<div\space=!<div\space*>>class\="signature"=.+' '' $bodydiv>
			<$bodydiv=$ret>
		</loop>
		<add_cleanup inouthtml=$&bodydiv>
		<gettagtextorhtml tag=$bodydiv istext=0 textret=$&htmlbody>
		<gettagtextorhtml tag=$bodydiv istext=1 textret=$&postbody>
		<!-- DATE -->
		<$daterex='>><td class\="message-user-info"=!<br+<br=!>*>='>
		<rex max=1 $daterex $postdata></rex>
		<fetch localhost/index.html $ret>
		<urltext>
		<$lastpostdate=$ret>
		<trimmer inout=$&lastpostdate>
		<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
		<!-- SIGNATURE -->
		<rex max=1 '<div\space=!<div\space*>>class\="signature"=.+' $postdata></rex>
		<getdivs data=$ret divs=$&divs>
		<loop max=1 $divs>
			<rex max=1 '>>>\P=.+' $divs></rex>
			<$signature=$ret>
		</loop>
		<!-- SITEPOSTID - using anchor value by default -->
		<loop max=1 $anchor><$sitepostid=$anchor></loop>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $HASUNIQUESITEPOSTID eq 1 and $sitepostid eq '' >
INVALID POST - SITEPOSTID REQUIRED
		<elseif $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $FIRSTPAGE eq 1 and $rexloop eq 0>
				<$threadstarter=1>
			<else>
				<$threadstarter=0>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$postbody
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=17>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>
<!-------------------------------------------------------------------------
--------------------------------------------------------------------------->
<a name=getanchor private data>
	<!--clean carriage returns, added By Tamer 06/01/04-->
	<$newline = "\n" "\r">
	<$replacement = "" "">
	<sandr $newline $replacement $data>
	<$data = $ret>
	<rex row max=1 ">><a=\space+name\=\P=!>*\F>" $data></rex>
	<sandr '"' '' $ret>
	<$anchor=$ret>
	<trimmer inout=$&anchor>
</a>

<!-------------------------------------------------------------------------
--------------------------------------------------------------------------->
<a name=getauthorurl private>
	<$rexauthorurl="action\=profile">
	<urllinks>
	<$links=$ret>
	<loop $links>
		<rex $rexauthorurl $links>
		<if $ret ne "">
			<$authorurl=$links>
			<break>
		</if>
	</loop>
</a>

<!-------------------------------------------------------------------------
--------------------------------------------------------------------------->
<a name=getbody private celldata>
<$bodyends =
			 ">>>\P=!________________*\F________________"
			 ">>>\P=!__________________*\F__________________"
			 ">>>\P=!_____________________________*\F_____________________________"
			 ">>>\P=!Report\spacethis\spacepost*\FReport\spacethis\spacepost"
			 ">>>\P=!Meld\spacedeze\spacepost*\FMeld\spacedeze\spacepost"
			 ">>>\P=!\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-*\F\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-"
			 ">>>\P=!\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-*\F\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-"
>
	<$bodyendfound=0>
	<loop $bodyends>
		<rex $bodyends $celldata>
		<$rexret=$ret>
		<if $rexret ne "">
			<$fetchcol=$rexret>
			<$bodyendfound=1>
			<break>
		</if>
	</loop>
	<if $bodyendfound eq 0>
		<rex row ">>>\P=.+" $celldata>
			<if $next eq 1>
				<$fetchcol=$ret>
			</if>
		</rex>
	</if>
	<$htmlbody=$fetchcol>
	<fetch localhost/index.html $htmlbody>
	<$bodydata=$ret>
	<urltext>
	<$postbody=$ret>
	<removequotes bodydata=$bodydata bodynoquotes=$&bodynoquotes>
</a>

<a name=clearpostrecbuffer private>
	<$author=>
	<$authorurl=>
	<$posttitle=>
	<$lastpostdate=>
	<$postbody=>
	<$bodynoquotes=>
	<$anchor=>
	<$authorloc=>
	<$gender=>
	<$authorage=>
	<$invalidfound=0>
	<$htmlbody=>
	<$signature=>
	<$sitepostid=>
	<$registered=>
	<$recommendation=>
	<$avatarurl=>
</a>


<a name=debug2>
	threadstarter: $threadstarter
	lastpostdate: $lastpostdate
	author: $author
	authorurl: <fmt "%s\r\n" $authorurl>
	avatarurl: <fmt "%s\r\n" $avatarurl>
	anchor:$anchor
	sitepostid: $sitepostid
	postbody: <fmt "%s\r\n" $postbody>
	<!--htmlbody: <fmt "\r\n%s\r\n" $htmlbody>-->
	signature: <fmt "%s\r\n" $signature>
</a>

<a name=debug>
	-------------------------------------------
	<br>
	author: $author
	<br>
	lastpostdate: $lastpostdate
	<br>
	posttitle: $posttitle
	<br>
	postbody:$postbody
	<br>
	signature:<send $signature>

	<br>
	anchor:$anchor
	<br>
	sitepostid: $sitepostid
	<br>
	authorloc:$authorloc
	<br>
	authorurl:$authorurl
	<br>
	avatarurl:$avatarurl
	<br>
	registered: $registered
	<br>
	gender:$gender
	<br>
	age:$authorage
	<br>
	recommendation:$recommendation
	<br>
	invalidfound:$invalidfound
	<br>
	-------------------------------------------
</a>

<a name=fixdate private inoutdate>
	<rex '\digit+H\space=\digit+mn\space=' $inoutdate>
	<if $ret ne "">
		<sandr 'H\space=' ':' $inoutdate>
		<sandr 'mn\space=.+' '' $ret>
		<$inoutdate=$ret>
	</if>
	<if $SITEDOMAIN eq 'forum.toyota-allion.ru'>
		<sandr 'òÿáðÿ' '' $inoutdate>
		<sandr 'ðÿ' '' $ret>
		<$inoutdate=$ret>
	</if>
	<if $SITEDOMAIN eq 'www.kostky.org' or $SITEDOMAIN eq 'diskuse.elektrika.cz'>
		<sandr '\spacev\space' ' ' $inoutdate>
		<sandr '\xBB' '' $ret>
		<sandr '\x3F' '' $ret>
		<sandr '\xC2' '' $ret>
		<$inoutdate=$ret>
	</if>
</a>
<a name=fixdate2 private inoutdate dateformat>
	<local change oldInOutDate timePart>
	<$oldInOutDate = $inoutdate>
	<if $LANG eq "Slovak">
		<$change = 1>
		<rex 'V pondelok' $inoutdate>
		<if $ret neq ''>
			<$inoutdate = 'Last Monday'>
		<else>
			<rex 'V utorok' $inoutdate>
			<if $ret neq ''>
				<$inoutdate = 'Last Tuesday'>
			<else>
				<rex 'V stredu' $inoutdate>
				<if $ret neq ''>
					<$inoutdate = 'Last Wednesday'>
				<else>
					<rex 'V Å¡tvrtok' $inoutdate>
					<if $ret neq ''>
						<$inoutdate = 'Last Thursday'>
					<else>
						<rex 'V piatok' $inoutdate>
						<if $ret neq ''>
							<$inoutdate = 'Last Friday'>
						<else>
							<rex 'V sobotu' $inoutdate>
							<if $ret neq ''>
								<$inoutdate = 'Last Saturday'>
							<else>
								<rex 'V nedeÄ¾u' $inoutdate>
								<if $ret neq ''>
									<$inoutdate = 'Last Sunday'>
								<else>
									<$change = 0>
								</if>
							</if>
						</if>
					</if>
				</if>
			</if>
		</if>
	</if>
	<if $change eq 1>
CHANGE HAPPEN($oldInOutDate, $dateformat)
                <$inoutdate=(convert($inoutdate, 'date'))>
new date: $inoutdate
		<if $dateformat eq "10003">
			<strfmt '%d.%m.%Y %H:%M:%S' $inoutdate>
			<$inoutdate=$ret>
		</if>
FORMATTED new date: $inoutdate
		<rex max=1 '\digit+:=\digit+:?\digit*' $oldInOutDate></rex>
		<$timePart = $ret>
		<if $timePart neq ''>
			<sandr '\digit+:=\digit+:?\digit*' $timePart $inoutdate>
			<$inoutdate = $ret>
		</if>
	</if>
</a>

<a name=fixregdate private passdate>
	<if $REGISTRATIONDATEFORMATID eq '10005'>
		<!-- first check if date already includes the day -->
		<rex '\alpha\space=\digit+\,?\space=\digit+' $passdate>
		<if $ret eq ''>
			<!-- locate the year -->
			<rex '\digit\digit\digit\digit' $passdate>
			<$holdyear=$ret>
			<strfmt "%s%s" '01 ' $holdyear>
			<!-- repalce the year with the string consisting of the day and year -->
			<sandr $holdyear $ret $passdate>
			<$passdate=$ret>
		</if>
	</if>
	<if $SITEDOMAIN eq 'www.forumfotografia.net'>
		<!-- no date format exists for this format (mm/yyyy), convert it to mm/dd/yyyy -->
		<rex '\digit/\digit' $passdate>
		<if $ret ne ''>
			<sandr '/' '/01/' $passdate>
			<$passdate=$ret>
		</if>
	</if>
	<if $SITEDOMAIN eq 'www.chevyavalancheclub.com'>
			<sandr '\.' '/' $passdate>
			<$passdate=$ret>
	</if>
	<if $SITEDOMAIN eq 'talk.uk-yankee.com'>
			<sum '%s' '01 ' $passdate>
			<$passdate=$ret>
	</if>
</a>

<a name=add_cleanup private inouthtml>
	<if $SITEDOMAIN eq 'www.cyclingtopics.com' or $SITEDOMAIN eq 'www.satellite-audiovideo.com'>
		<sandr '>>You are not allowed to view links=!">Login</a>+">Login</a>=' '' $inouthtml>
		<$inouthtml=$ret>
	</if>
	<if $SITEDOMAIN eq 'www.helpwebhosting.com' or $SITEDOMAIN eq 'chevysonic-club.com'>
		<sandr '>><div class\="sharethis_=.+' '' $inouthtml>
		<$inouthtml=$ret>
	</if>
	<if $SITEDOMAIN eq 'forum.pravda.com.ua'>
		<sandr '<em=!<em+>>>ÐžÑÑ‚Ð°Ð½Ð½Ñ” Ñ€ÐµÐ´Ð°Ð³ÑƒÐ²Ð°Ð½Ð½Ñ:=!</em>+</em>=' '' $inouthtml>
		<$inouthtml=$ret>
	</if>
</a>

<a name=domain_blocker private callingmethod myreturn>
	<!-- has effect in debug mode only - prevent certain parse-methods to process certain websites (domains).
	useful in test/debug mode to determine the best method for parsing.
	In normal mode the variable values should always be blank so this fn doesn't have effect. -->
	<$myreturn=0>
	<$thismethod=''>
	<$thisdomain=''>
	<loop $thismethod>
		<if $thismethod eq $callingmethod>
			<if $SITEDOMAIN	eq $thisdomain>
				<$myreturn=1>
			</if>
		</if>
	</loop>
</a>
</script>




