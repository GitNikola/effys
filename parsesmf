<script language=vortex>
<uses bbsrex gethtml checkdate insertpost>

<a name=smfparseposts export threadbody insertedthreadid forumid siteid urlthreadid basethreadurl=''
	link linktosite hotlink sitealias siteurl bbstype bbsver threadtitle forumlastcrawled
	lang cacheposts forumname daysbacktocrawl forumurl sitetitle charset firstpage
	gmt='' adult=0 country='' backwardsthread=0 domain='' registrationdateformatid=''
	hasuniquesitepostid=0 feedsources='' sitetopic='' forumcategories='' forumtopics=''
	providerid=''
	parsemethod totalposts allposts=0 postsparsed timeformatid
	dupsfound>
	<$THREADBODY=$threadbody>
	<$INSERTEDTHREADID=$insertedthreadid>
	<$FORUMID=$forumid>
	<$SITEID=$siteid>
	<$URLTHREADID=$urlthreadid>
	<$THREADURL=$link>
	<$LINKTOSITE=$linktosite>
	<$SITEALIAS=$sitealias>
	<$SITEURL=$siteurl>
	<$HOTLINK=$hotlink>
	<$BBSTYPE=$bbstype>
	<$BBSVER=$bbsver>
	<$THREADTITLE=$threadtitle>
	<$FORUMLASTCRAWLED=$forumlastcrawled>
	<$LANG=$lang>
	<$CACHEPOSTS=$cacheposts>
	<$PARSEMETHOD=$parsemethod>
	<$ORIGPARSEMETHOD=$parsemethod>
	<$POSTSPARSED=$postsparsed>
	<$TIMEFORMATID=$timeformatid>
	<$FORUMNAME=$forumname>
	<$DAYSBACKTOCRAWL=$daysbacktocrawl>
	<$BASETHREADURL=$basethreadurl>
	<sum "%s" "-" $DAYSBACKTOCRAWL " days">
	<$MAXDAYS=$ret>
	<$FORUMURL=$forumurl>
	<$SITETITLE=$sitetitle>
	<$CHARSET=$charset>
	<$FIRSTPAGE=$firstpage>
	<$GMT=$gmt>
	<$ADULT=$adult>
	<$COUNTRY=$country>
	<$REGISTRATIONDATEFORMATID=$registrationdateformatid>
	<$HASUNIQUESITEPOSTID=$hasuniquesitepostid>
	<$FEEDSOURCES=$feedsources>
	<$SITETOPIC=$sitetopic>
	<$FORUMCATEGORIES=$forumcategories>
	<$FORUMTOPICS=$forumtopics>
	<$PROVIDERID=$providerid>
	<!-- don't keep alt text from images -->
	<urlcp alttxt no>
	<!-- initialize order to 1 for posts -->
	<$POSTORDER=1>
	<!-- parse posts -->
	<$POSTSCNT=0>
	<$ALLPOSTSCNT=0>
	<$DUPSFOUND=0>
	<parsebody>
	<$parsemethod=$PARSEMETHOD>
	<$totalposts=$POSTSCNT>
	<$allposts=$ALLPOSTSCNT>
	<$postsparsed=$POSTSPARSED>
	<$timeformatid=$TIMEFORMATID>
	<$dupsfound=$DUPSFOUND>
</a>

