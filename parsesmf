						                <sandr 'Â»' "" $ret>
                                                        </if>                                                                                                                                                                
							<$lastpostdate=$ret>
							<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
						<else>
	D2
							<$spdays="Lunes, " "Martes, " "Miércoles, " "Jueves, " "Viernes, " "Domingo, " "Sábado, ">
							<if $SITEDOMAIN eq 'ceedclub.ru'>
								<rex row max=1 '>>ÐžÑ‚Ð²ÐµÑ‚=' $divcontent></rex>
								<if $ret neq ''>
									<rex row max=1 ':\P=.+' $divcontent></rex>
								<else>
									<rex row max=1 '>>=.+' $divcontent></rex>
								</if>
							<else>
								<rex ':\P=.+' $divcontent>
							</if>
							<sandr $spdays "" $ret>
							<sandr "\spacede\space" "-" $ret>
							<sandr "\spaceklo:" " " $ret>
							<sandr " Ã  " " " $ret>
							<sandr ' â ' ' ' $ret>
							<sandr ' Ð² ' ' ' $ret>
							<sandr ' às ' ' ' $ret>
							<if $LANG eq "Swedish" or $LANG eq "Norwegian">
								<sandr ' kl\.' '' $ret>
								<sandr '\. ' '.' $ret>
							<elseif $LANG eq "Spanish">
								<sandr ' a las ' ' ' $ret>
							<elseif $LANG eq "Russian">
								<sandr "Â»" "" $ret>
								<sandr '»' "" $ret>
								<sandr ' Ð² ' " " $ret>
								<sandr 'Â«' "" $ret>
								<sandr 'Â»' "" $ret>
							</if>
							<if $SITEDOMAIN eq 'forum.merelbeke.net'>
								<sandr ' - ' ' ' $ret>
							</if>
