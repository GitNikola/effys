							<if $SITEDOMAIN eq 'mama51.ru'>
								<!-- convert dd, mm, yyyy, hh:mm:ss to dd.mm.yyyy hh:mm:ss -->
								<sandr '>>\digit\digit\P=\, =\F\digit\digit\, \digit\digit\digit\digit=' '.' $lastpostdate>
								<sandr '>>\digit\digit\.\digit\digit\P=\, =\F\digit\digit\digit\digit=' '.' $ret>
								<$lastpostdate=$ret>
							<elseif $SITEDOMAIN eq 'baby.dn.ua'>
								<!-- move time from front to end of string -->
								<rex max=1 '\digit+:=\digit+' $lastpostdate></rex>
								<$holdtime=$ret>
								<sandr '\digit+:=\digit+' '' $lastpostdate>
								<sum "%s" $ret ' ' $holdtime>
								<$lastpostdate=$ret>
							</if>
							<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
						</if>
					</loop>
