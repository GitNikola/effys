<a name=parsemethod1 private textdata>
method1
	<domain_blocker callingmethod='1' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>
	<rex row '>><a\spacename\="msg=!<a\spacename\="msg+' $textdata>
		<$postdata=$ret>
		<$rexloop=$loop>
		<clearpostrecbuffer>
		<getanchor data=$postdata>
		<$posttable=>
		<rex '<table=!<table+>><span\spacestyle\="font\-size:\spacex\-small;"><b>#=.+' $postdata>
		<gettables data=$ret tables=$&tables>
		<loop max=2 $tables>
			<if $next eq 1>
				<$posttable=$tables>
			<elseif $next eq 2>
				<$postbodytable=$tables>
			</if>
		</loop>
		<rex row '<a\space=!<a\space+>>action\=profile=!</a>+</a>' $postdata>
			<$linkdata=$ret>
			<fetch $THREADURL $linkdata>
			<urltext>
			<$author=$ret>
			<trimmer inout=$&author>
			<if $author ne "">
				<urllinks>
				<loop max=1 $ret>
					<$authorurl=$ret>
				</loop>
				<break>
			</if>
		</rex>
		<rex '>><span\spacestyle\="font\-size:\spacex\-small;=.+' $posttable>
		<getspans data=$ret spans=$&spans>
		<loop max=1 $spans>
			<rex ">\P=.+" $spans>
			<fetch localhost/index.html $ret>
			<urltext>
			<$lastpostdate=$ret>
			<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
		</loop>
		<getcells data=$postbodytable cells=$&cells>
		<loop max=2 $cells>
			<if $next eq 2>
				<rex ">\P=.+" $cells>
				<$htmlbody=$ret>
				<fetch localhost/index.html $htmlbody>
				<urltext>
				<$postbody=$ret>
				<removequotes bodydata=$postbody bodynoquotes=$&bodynoquotes>
			</if>
		</loop>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $FIRSTPAGE eq 1 and $rexloop eq 0>
				<$threadstarter=1>
			<else>
				<$threadstarter=0>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$bodynoquotes
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter  signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=1>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>

<a name=parsemethod2 private textdata>
method2
	<domain_blocker callingmethod='2' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>
	<rex row '>><a\spacename\="=!<table+<table=!<a\spacename\="+' $textdata>
		<$postdata=$ret>
		<$rexloop=$loop>
		<clearpostrecbuffer>
		<getanchor data=$postdata>
		<$posttable=>
		<gettables data=$postdata tables=$&tables>
		<loop $tables>
			<rex 'class\="postbit"' $tables>
			<if $ret ne "">
				<$posttable=$tables>
				<break>
			</if>
		</loop>
		<$postrow=>
		<getrows data=$posttable rows=$&rows>
		<loop max=1 $rows>
			<$postrow=$rows>
		</loop>
		<getcells data=$postrow cells=$&cells>
		<loop $cells>
			<if $next eq 1>
				<gettables data=$cells tables=$&tables>
				<$authortable=>
				<loop max=1 $tables>
					<$authortable=$tables>
				</loop>
				<rex ">\P=.+" $authortable>
				<fetch localhost/index.html $ret>
				<urltext>
				<$author=$ret>
				<trimmer inout=$&author>
			<elseif $next eq 2>
				<gettables data=$cells tables=$&tables>
				<$datetable=>
				<loop max=1 $tables>
					<$datetable=$tables>
				</loop>
				<getcells data=$datetable cells=$&cells2>
				<loop max=1 $cells2>
					<rex ">\P=.+" $cells2>
					<$coldata=$ret>
					<fetch localhost/index.html $coldata>
					<urltext>
					<$lastpostdate=$ret>
					<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
				</loop>
				<rex '<div\spaceclass\="p_subject">\P=!</div+\F</div' $cells>
				<fetch localhost/index.html $ret>
				<urltext>
				<$posttitle=$ret>
				<trimmer inout=$&posttitle>
				<rex '<div\spaceclass\="postbody"\P=!</div+\F</div' $cells>
				<rex ">\P=.+" $ret>
				<$bodydata=$ret>
				<$htmlbody=$bodydata>
				<fetch localhost/index.html $htmlbody>
				<urltext>
				<$postbody=$ret>
				<removequotes bodydata=$bodydata bodynoquotes=$&bodynoquotes>
			</if>
		</loop>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $FIRSTPAGE eq 1 and $rexloop eq 0>
				<$threadstarter=1>
			<else>
				<$threadstarter=0>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$bodynoquotes
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=2>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>

<a name=parsemethod6 private textdata>
method6
	<domain_blocker callingmethod='6' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>
	<$rexloop=0>
	<$mainrex = '>><a\spacename\="msg=!<a\spacename\="msg+' '>><a\spaceid\="msg=\digit+"=!<a\spaceid\="msg+' >
	<if $SITEDOMAIN eq 'www.offtopicalia.com'>
		<$mainrex = '<table=!<table+>><a\spacename\="msg=!<a\spacename\="msg+' >
	</if>
	<if $SITEDOMAIN eq 'www.desdeunlugarmejor.com'>
		<$mainrex = '<table=!<.....+class\="catbg"=!class\="catbg"+>>id\="subject_=!id\="subject_+'>
	</if>
	<rex row $mainrex $textdata>
		<$postdata=$ret>
		<!--***************************
		<send $postdata>

		***************************-->
		<clearpostrecbuffer>
		<getanchor data=$postdata>
		<if $SITEDOMAIN eq 'www.offtopicalia.com' or $SITEDOMAIN eq 'www.vatersein.de'>
			<rex max=1 '>><a\spacename\="\P=msg=\digit+' $postdata></rex>
			<$anchor=$ret>
			<$originalpostdata=$postdata>
		</if>
		<if $SITEDOMAIN eq 'www.desdeunlugarmejor.com'>
			<rex max=1 '>>id\="subject_\P=\digit+' $postdata></rex>
			<if $ret neq ''>
				<sum '%s' 'msg' $ret>
				<$anchor=$ret>
				<$originalpostdata=$postdata>
			</if>
		</if>
		<if $SITEDOMAIN eq 'www.pizzamaking.com'>
			<rex max=1 '>><a\spacename\="\P=msg=\digit+' $postdata></rex>
			<$anchor=$ret>
		</if>
		<!-- this ain' gonna work:
		<rex max=1 '>><td\spaceclass\="windowbg=.+' $postdata></rex>
		there are examples where there is more than one <TD of a "windowbg" class.
		first such cell we don't want to pick up because it's some header data, not the post content -->
		<$temp=''>
		<rex row '>><td\spaceclass\="windowbg=!<td\spaceclass\="windowbg+' $postdata>
			<$temp=$ret>
			<!-- this way we're collecting the last <TD> of desired class -->
		</rex>
		<if $SITEDOMAIN eq 'www.svenskamord.se' or $SITEDOMAIN eq 'forocistierna.es' or $SITEDOMAIN eq 'www.desdeunlugarmejor.com'
			or $SITEDOMAIN eq 'www.gradoadeuned.com' or $SITEDOMAIN eq 'www.debatimos.com' or $SITEDOMAIN eq 'www.pizzamaking.com'
			or $SITEDOMAIN eq 'forum.calgaryjeep.com' or $SITEDOMAIN eq 'caddychat.com' or $SITEDOMAIN eq 'www.iqcolony.com'>
			<rex max=1 '>><td\spaceclass\="windowbg=.*' $postdata></rex>
			<$temp=$ret>
		</if>
		<if $SITEDOMAIN eq 'ceedclub.ru'>
			<rex max=1 '>><td\spaceclass\="windowbg[^\space]=.*' $postdata></rex>
			<$temp=$ret>
		</if>
		<!-- pools in 'www.terrainforum.net', we have to skip "poll" & "advertisement" cell with class="windowbg to catch the 1st post on a page-->
		<rex max=1 '>>_poll\.gif"' $postdata></rex>
		<if $ret neq ''  and $SITEDOMAIN eq 'www.terrainforum.net' and  $rexloop eq 0 >
			<rex row '>><td\spaceclass\="windowbg=[^3]=!<td\spaceclass\="windowbg+' $postdata>
				<$temp=$ret>
			</rex>
		<else>
			<rex max=1 '>><td\spaceclass\="windowbg=.*' $postdata></rex>
			<$temp=$ret>
		</if>
		<$postdata=$temp>
		<$postrow1=>
		<getrows data=$postdata rows=$&rows>
		<loop max=1 $rows>
			<$postrow1=$rows>
		</loop>
		<if $postrow1 eq "">
			<rex '<tr=!<tr+>>action\=profile=.+' $postdata>
			<$postrow1=$ret>
		</if>
		<rex '>><div\spaceclass\="post"=.+' $postdata>
		<getdivs data=$ret divs=$&divs>
		<loop max=1 $divs>
			<rex ">\P=.+" $divs>
			<$htmlbody=$ret>
			<!-- clean-up and signature -->
			<if $SITEDOMAIN eq 'foroscastilla.org'>
				<sandr '>><div id\="share_this"=!</div>+</div>=!</div>*</div>=' '' $htmlbody>
				<$htmlbody=$ret>
			</if>
			<if $SITEDOMAIN eq 'www.thecore.es'>
				<sandr '<div =!<div +>>((Para ver los links))=!</div>+</div>=' '' $htmlbody>
				<$htmlbody=$ret>
			</if>
			<sandr '<small><i>Sponsorlu Baðlantýlar</i></small>' '' $htmlbody>
			<$htmlbody=$ret>
			<rex '>><br />_________________<br />\P=.+' $htmlbody>
			<$signature=$ret>
			<sandr '>><br />_________________<br />=.+' '' $htmlbody>
			<$htmlbody=$ret>
			<fetch localhost/index.html $htmlbody>
			<urltext>
			<$postbody=$ret>
			<trimmer inout=$&postbody>
			<removequotes bodydata=$bodydata bodynoquotes=$&bodynoquotes>
		</loop>
		<getcells data=$postrow1 cells=$&cells>
		<loop $cells>
			<$cellnext=$next>
			<if $cellnext eq 1>
				<rex '.+\F<div\spaceclass\="smalltext' $cells>
				<rex row max=1 '>><a\space=!</a>+</a>=' $ret></rex>
				<if $SITEDOMAIN eq 'www.svenskamord.se'>
					<rex max=1 '<a\space=!<a\space+>>=' $ret></rex>
				<elseif $SITEDOMAIN eq 'forum.maistrafego.pt'>
					<rex max=1 '<a\space=!<a\space+>>class\="postername"=!</a>+</a>=' $postdata></rex>
				<elseif $SITEDOMAIN eq 'forum.free2talk.de' or $SITEDOMAIN eq 'forum.rostovmama.ru'>
					<rex max=1 '<a\space=!<a\space+>>\?action\=profile;u\==!</a>+</a>=' $postdata></rex>
				</if>
				<fetch $THREADURL $ret>
				<urltext>
				<$author=$ret>
				<trimmer inout=$&author>
				<if $author ne "">
					<urllinks>
					<$authorurl=$ret>
				<else>
					<rex ">\P=.+" $cells>
					<rex '.+\F<div\spaceclass\="smalltext' $ret>
					<fetch localhost/index.html $ret>
					<urltext>
					<$author=$ret>
					<trimmer inout=$&author>
					<if $author eq "">
						<rex row max=1 '>><a\space=!</a>+</a>=' $cells></rex>
						<fetch $THREADURL $ret>
						<urltext>
						<$author=$ret>
						<trimmer inout=$&author>
						<if $author ne "">
							<urllinks>
							<$authorurl=$ret>
						</if>
					</if>
					<if $author eq "">
						<rex row max=1 '>><b>\P=!</b>+' $cells></rex>
						<!--<fetch $THREADURL $ret>
						<urltext>-->
						<$author=$ret>
						<!--<trimmer inout=$&author>-->
					</if>
					<if $author eq "" and $SITEDOMAIN eq 'diskuse.elektrika.cz'>
						<$author='Anonymous'>
					</if>
				</if>
				<rex '>>Gender:\P=!<br+\F<br' $cells>
				<$genderhtml=$ret>
				<if $genderhtml eq "">
					<$sexrex='>>Sesso:\P=!<br+\F<br' '>>Sexe:\P=!<br+\F<br' '>>Geschlecht:\P=!<br+\F<br'
						'>>Sukupuoli:\P=!<br+\F<br' '>>Sexo:\P=!<br+\F<br' '>>Geslacht:\P=!<br+\F<br'
						'>>Kön:=\space*<img\space=!>+>' '>>K&ouml;n:=\space*</b>?\space*<img\space=!>+>'
						'>>KÃ¸n:=\space*<img\space=!>+>' '>>Kj&oslash;nn:\P=!<br+' '>>Ïîë:\P=!<br+'
						'>>ÐŸÐ¾Ð»:\P=!<br+' '>>Pohlaví:=\space*<img\space=!>+>'>
					<rex $sexrex $cells>
					<$genderhtml=$ret>
				</if>
				<if $SITEDOMAIN eq 'www.latestbingobonuses.com'>
					<!-- check the whole cell -->
					<$genderhtml=$cells>
				</if>
				<rex 'Female' $genderhtml>
				<if $ret ne "">
					<$gender="F">
				<else>
					<rex 'Male' $genderhtml>
					<if $ret ne "">
						<$gender="M">
					</if>
				</if>
				<if $gender eq ''>
					<rex 'images/\P=fe?male=\F\.gif"=' $genderhtml>
					<if $ret eq 'female'>
						<$gender="F">
						<$genderfound=1>
					<elseif $ret eq 'male'>
						<$gender="M">
						<$genderfound=1>
					</if>
				</if>
				<$agerex='>>Age:\P=!<br+' '>>Al[dt]er:\P=!<br+' '>>Idade:\P=!<br+' '>>Âîçðàñò:\P=!<br+'>
				<rex $agerex $cells>
				<fetch localhost/index.html $ret>
				<urltext>
				<$authorage=$ret>
				<trimmer inout=$&authorage>
				<rex '\digit+' $authorage>
				<$authorage=$ret>
				<$locrex= '>>Localización: \P=!<br+'    '>>Location:\P=!<br+' '>>Local:\P=!<br+' '>>Lugar:\P=!<br+'
					'>>Herkunft:\P=!<br+' '>>Ðàñïîëîæåíèå:\P=!<br+' '>>Ort:\P=!<br+' '>>ÐžÑ‚ÐºÑƒÐ´Ð°:\P=!<br+'
					'>>>Ð ÐµÐ³Ð¸Ð¾Ð½</b>:\P=!<br+' '>>Ð“Ð¾Ñ€Ð¾Ð´:\P=!<+' '>>Ãîðîä\P=!<+'>
				<if $SITEDOMAIN eq 'www.foro.todomecanica.com' or $SITEDOMAIN eq 'www.as-fas.es'
					or $SITEDOMAIN eq 'ceedclub.ru'>
					<$locrex='<img=!<...+>>/images/flags/=!alt\="+alt\="\P=!"+'>
				<elseif $SITEDOMAIN eq 'forum.dailymobile.se'>
					<$locrex='>>Location:\P=!<br />+'>
				<elseif $SITEDOMAIN eq 'www.paylashocam.com'>
					<$locrex='>><span=!>*>=\space*Yer:=\space*</span>\P=!<+'>
				<elseif $SITEDOMAIN eq 'www.byggebolig.no'>
					<$locrex='>>Bosted:\P=!<br+'>
				</if>
				<rex $locrex $cells>
				<fetch localhost/index.html $ret>
				<urltext>
				<$authorloc=$ret>
				<trimmer inout=$&authorloc>
				<if $SITEDOMAIN eq 'ceedclub.ru' or $SITEDOMAIN eq 'www.daewoo-park.ru' or $SITEDOMAIN eq 'www.agrarweb.at'>
					<rex '<img=!<...+>>/images/flags/=!>+>\P=!<+' $cells>
					<fetch localhost/index.html $ret>
					<urltext>
					<$temp = $ret>
					<trimmer inout=$&temp>
					<sum '%s' $authorloc ' ' $temp>
					<$authorloc=$ret>
				</if>
				<!-- AVATAR -->
				<$avatarrex='<img=!<img+>>class\="avatar=!>+>=' '<img=!<img+>>/avatars/=!>+>='
							'<img=!<img+>>/galleria/albums/userpics/=!>+>=' '<img=!<img+>>class\="avatar"=!>+>='>
				<$avreject='no\-avatar\.jpg'>
				<rex max=1 $avatarrex $cells></rex>
				<getimagesrc htmlsrc=$ret domain=$THREADURL srcret=$&avatarurl reject=$avreject>
			<elseif $cellnext eq 2>
				<rex '<div=!<div+>>id\="subject_=.+' $cells>
				<getdivs data=$ret divs=$&divs>
				<loop max=1 $divs>
					<rex ">\P=.+" $divs>
					<fetch localhost/index.html $ret>
					<urltext>
					<$posttitle=$ret>
					<trimmer inout=$&posttitle>
				</loop>
				<if $posttitle eq "">
					<rex row '>><a\space=!</a>+</a>=' $cells>
						<$htmllink=$ret>
						<rex "#" $htmllink>
						<if $ret ne "">
							<fetch localhost/index.html $htmllink>
							<urltext>
							<$posttitle=$ret>
							<trimmer inout=$&posttitle>
							<if $posttitle ne "">
								<break>
							</if>
						</if>
					</rex>
				</if>
