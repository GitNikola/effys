	</rex>
</a>

<a name=parsemethod3 private textdata>
method3
	<domain_blocker callingmethod='3' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>
	<$rexloop=0>
	<$rexpattern='>><td\spaceclass\="windowbg=!<td\spaceclass\="windowbg+'>
	<if $SITEDOMAIN eq 'www.egitim-forum.com'>
		<$rexpattern='>><td\spaceclass\="windowbg=[^3]=!<td\spaceclass\="windowbg+'>
	</if>
	<rex row $rexpattern $textdata>
		<$postdata=$ret>
		<clearpostrecbuffer>
		<if $SITEDOMAIN neq 'waronyou.com'>
			<getanchor data=$postdata>
		</if>
<!--****************************************************
<send $postdata>

****************************************************-->
		<$postrow1=>
		<getrows data=$postdata rows=$&rows>
		<loop max=1 $rows>
			<$postrow1=$rows>
		</loop>
		<if $postrow1 eq "">
			<rex '<tr=!<tr+>>action\=profile=.+' $postdata>
			<$postrow1=$ret>
		</if>
		<getcells data=$postrow1 cells=$&cells>
		<loop $cells>
			<$cellnext=$next>
			<if $cellnext eq 1>
				<rex '.+\F<div\spaceclass\="smalltext' $cells>
				<rex row max=1 '>><a\space=!</a>+</a>=' $ret></rex>
				<fetch $THREADURL $ret>
				<urltext>
				<$author=$ret>
				<trimmer inout=$&author>
				<if $author ne "">
					<urllinks>
					<$authorurl=$ret>
				<else>
					<rex ">\P=.+" $cells>
					<rex '.+\F<div\spaceclass\="smalltext' $ret>
					<fetch localhost/index.html $ret>
					<urltext>
					<$author=$ret>
					<trimmer inout=$&author>
					<if $author eq "">
						<rex row max=1 '>><a\space=!</a>+</a>=' $cells></rex>
						<fetch $THREADURL $ret>
						<urltext>
						<$author=$ret>
						<trimmer inout=$&author>
						<if $author ne "">
							<urllinks>
							<$authorurl=$ret>
						</if>
					</if>
				</if>
				<rex '>>Gender:\P=!<br+\F<br' $cells>
				<$genderhtml=$ret>
				<if $genderhtml eq "">
					<rex '>>Sesso:\P=!<br+\F<br' $cells>
					<$genderhtml=$ret>
				</if>
				<rex 'Female' $genderhtml>
				<if $ret ne "">
					<$gender="F">
				<else>
					<rex 'Male' $genderhtml>
					<if $ret ne "">
						<$gender="M">
					</if>
				</if>
				<$malerex='Male\.gif'>
				<$femalerex='Female\.gif'>
				<rex $malerex $postdata>
				<if $ret ne ''><$gender="M"></if>
				<rex $femalerex $postdata>
				<if $ret ne ''><$gender="F"></if>
				<!-- AVATAR -->
				<$avatarrex='<img=!<img+>>class\="avatar"=!>+>=' '<img=!<img+>>/avatars/=!>+>='
							'<img=!<img+>>/galleria/albums/userpics/=!>+>=' '<img=!<...+>>type\=avatar=!>+>='
							'<img=!<img+>>/picture\?type\=normal=!>+>='>
				<rex max=1 $avatarrex $cells></rex>
				<getimagesrc htmlsrc=$ret domain=$THREADURL srcret=$&avatarurl>
	            <$locrex='>>Location:\P=!<br />+'>
				<rex $locrex $cells>
				<fetch localhost/index.html $ret>
				<urltext>
				<$authorloc=$ret>
				<trimmer inout=$&authorloc>
			<elseif $cellnext eq 2>
				<if $anchor eq "">
					<rex max=1 '>>msg=\digit+#\P=msg=\digit+' $cells></rex>
					<$anchor=$ret>
				</if>
				<rex '<div=!<div+>>id\="subject_=.+' $cells>
				<getdivs data=$ret divs=$&divs>
				<loop max=1 $divs>
					<rex ">\P=.+" $divs>
					<fetch localhost/index.html $ret>
					<urltext>
					<$posttitle=$ret>
					<trimmer inout=$&posttitle>
				</loop>
				<if $posttitle eq "">
					<rex row '>><a\space=!<a\space+</a>=' $cells>
						<$htmllink=$ret>
						<rex "#" $htmllink>
						<if $ret ne "">
							<fetch localhost/index.html $htmllink>
							<urltext>
							<$posttitle=$ret>
							<trimmer inout=$&posttitle>
							<if $posttitle ne "">
								<break>
							</if>
						</if>
					</rex>
				</if>
				<if $posttitle eq "">
					<rex '>><td\P=!<span\spaceclass\="smalltext+\F<span\spaceclass\="smalltext' $cells>
					<rex ">\P=.+" $ret>
					<fetch localhost/index.html $ret>
					<urltext>
					<$posttitle=$ret>
					<trimmer inout=$&posttitle>
				</if>
				<rex '>><div\spaceclass\="smalltext=.+' $cells>
				<getdivs data=$ret divs=$&divs>
				<loop max=1 $divs>
D1
					<rex ">\P=.+" $divs>
					<fetch localhost/index.html $ret>
					<urltext>
					<$divcontent=$ret>
					<rex ">>on:\P=.+" $divcontent>
					<if $ret ne "">
						<$lastpostdate=$ret>
						<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
					<else>
						<$spdays="Lunes, " "Martes, " "Miércoles, "Jueves, "Viernes, " "Domingo, " "Sábado, ">
						<rex ':\P=.+' $divcontent>
						<sandr $spdays "" $ret>
						<sandr "\spacede\space" "-" $ret>
						<sandr " â " " " $ret>
						<$lastpostdate=$ret>
						<fixdate inoutdate=$&lastpostdate>
						<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
					</if>
				</loop>
				<if $lastpostdate eq "">
D2
					<rex '>><span\spaceclass\="smalltext=.+' $cells>
					<getspans data=$ret spans=$&spans>
					<loop max=1 $spans>
						<rex ">\P=.+" $spans>
						<fetch localhost/index.html $ret>
						<urltext>
						<rex ">>on:\P=.+" $ret>
						<$lastpostdate=$ret>
						<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
					</loop>
				</if>
				<if $htmlbody eq "">
					<sandr '>><table=!</table>+</table>' '' $cells>
					<add_cleanup inouthtml=$&ret>
					<rex ">\P=.+" $ret>
					<$htmlbody=$ret>
					<fetch localhost/index.html $htmlbody>
					<urltext>
					<$postbody=$ret>
					<trimmer inout=$&postbody>
					<removequotes bodydata=$htmlbody bodynoquotes=$&bodynoquotes>
				</if>
			</if>
		</loop>
		<if $lastpostdate eq "">
D3
			<rex '<td=!<td+<table=!<table+>><div\spaceclass\="post"=.+' $postdata>
			<$celldata=$ret>
			<if $SITEDOMAIN eq 'www.creadoo.com'>
				<rex '>><span\spaceclass\="smalltext=!<td+\F<td' $celldata>
			<else>
				<rex '>><div\spaceclass\="smalltext=!<td+\F<td' $celldata>
			</if>
			<rex ">\P=.+" $ret>
			<fetch localhost/index.html $ret>
			<urltext>
			<$divcontent=$ret>
			<rex ">>on:\P=.+" $divcontent>
			<if $ret ne "">
				<$lastpostdate=$ret>
				<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
			<else>
				<$spdays="Lunes, " "Martes, " "Miércoles, "Jueves, "Viernes, " "Domingo, " "Sábado, ">
				<rex ':\P=.+' $divcontent>
				<sandr $spdays "" $ret>
				<sandr "\spacede\space" "-" $ret>
				<sandr ' um ' ' ' $ret>
				<sandr '\.' '' $ret>
				<$lastpostdate=$ret>
				<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
			</if>
			<if $posttitle eq "">
				<rex '>><td\P=!<div\spaceclass\="smalltext+\F<div\spaceclass\="smalltext' $celldata>
				<rex ">\P=.+" $ret>
				<fetch localhost/index.html $ret>
				<urltext>
				<$posttitle=$ret>
				<trimmer inout=$&posttitle>
			</if>
			<if $posttitle eq "">
				<rex '>><td\P=!<span\spaceclass\="smalltext+\F<span\spaceclass\="smalltext' $celldata>
				<rex ">\P=.+" $ret>
				<fetch localhost/index.html $ret>
				<urltext>
				<$posttitle=$ret>
				<trimmer inout=$&posttitle>
			</if>
		</if>
		<if $htmlbody eq "">
			<rex '>><div\spaceclass\="post"=.+' $postdata>
			<getdivs data=$ret divs=$&divs>
			<loop max=1 $divs>
				<add_cleanup inouthtml=$&divs>
				<rex ">\P=.+" $divs>
				<$htmlbody=$ret>
				<fetch localhost/index.html $htmlbody>
				<urltext>
				<$postbody=$ret>
				<trimmer inout=$&postbody>
				<removequotes bodydata=$htmlbody bodynoquotes=$&bodynoquotes>
			</loop>
		</if>
		<!-- SIGNATURE - sometimes in the 2nd row, so it must be extracted from $postdata -->
		<rex '<div=!<div*>>class\="signature"=.+' $postdata>
		<getdivs data=$ret divs=$&sigdivs>
		<loop max=1 $sigdivs>
			<gettagtextorhtml tag=$sigdivs istext=0 textret=$&signature>
		</loop>
		<if $REGISTRATIONDATEFORMATID ne '' AND $REGISTRATIONDATEFORMATID ne '0'>
R1
			<$authorregrex='>>>Ðåãèñòðàöèÿ:\P=!<+\F<'>
			<rex $authorregrex $postdata>
			<fetch localhost/index.html $ret>
			<urltext>
			<$registered=$ret>
			<trimmer inout=$&registered>
			<checkdate lang=$LANG isdateonly=1 indate=$&registered indateid=$REGISTRATIONDATEFORMATID registereddate=1>
		</if>
		<if $SITEDOMAIN eq 'forum.toyota-allion.ru'>
			<rex max=1 '<a=!<a+>>\?action\=profile;u=!</a>+</a>=' $postdata></rex>
			<fetch $u $ret>
			<urllinks>
			<$authorurl=$ret>
			<rex max=1 '>>msg=\digit+#\P=msg=\digit+' $postdata></rex>
			<$anchor=$ret>
		</if>
		<if $SITEDOMAIN eq 'www.creadoo.com' or $SITEDOMAIN eq 'forum.passioneauto.it' or $SITEDOMAIN eq 'www.go-android.de'
			or $SITEDOMAIN eq 'www.cogknowhow.com'>
			<rex max=1 '>>msg=\digit+/?#\P=msg=\digit+' $postdata></rex>
			<$anchor=$ret>
		</if>
		<!-- SITEPOSTID - using anchor value by default -->
		<if $HASUNIQUESITEPOSTID eq 1>
			<loop max=1 $anchor><$sitepostid=$anchor></loop>
		</if>
		<if $author eq 'Advertisement' or $author eq 'PakStudy : Study Matters in Pakistan'>
			<$author=''>
		</if>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
		<if $invalidfound eq 0>
			<$rexloop=($rexloop+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $HASUNIQUESITEPOSTID eq 1 and $sitepostid eq '' >
INVALID POST - SITEPOSTID REQUIRED
		<elseif $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $FIRSTPAGE eq 1 and $rexloop eq 1>
				<$threadstarter=1>
			<else>
				<$threadstarter=0>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$bodynoquotes
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				recommendation=""
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=3>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>

<a name=parsemethod4 private textdata>
method4
	<domain_blocker callingmethod='4' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>
	<rex row '<table=!<table+<table=!<table+>>action\=profile=!action\=profile+' $textdata>
		<$postdata=$ret>
		<$rexloop=$loop>
		<clearpostrecbuffer>
		<getanchor data=$postdata>
		<$posttable=>
		<gettables data=$postdata tables=$&tables>
		<loop max=1 $tables>
			<$posttable=$tables>
		</loop>
		<getrows data=$posttable rows=$&rows>
		<loop max=1 $rows>
			<getcells data=$rows cells=$&cells>
			<loop $cells>
				<$cellnext=$next>
				<rex ">\P=.+" $cells>
				<$coldata=$ret>
				<if $cellnext eq 1>
					<getlinktext hreftext="action\=profile" data=$coldata linktext=$&linktext>
					<$author=$linktext>
					<fetch $THREADURL $coldata>
					<getauthorurl>
				<elseif $cellnext eq 2>
					<getrows data=$coldata rows=$&rows2>
					<loop $rows2>
						<if $next eq 1>
							<getcells data=$rows2 cells=$&cells2>
							<loop $cells2>
								<if $next eq 2>
									<rex '<div=!<div+>>id\="subject_=.+' $cells2>
									<getdivs data=$ret divs=$&divs>
									<loop max=1 $divs>
										<rex ">\P=.+" $divs>
										<fetch localhost/index.html $ret>
										<urltext>
										<$posttitle=$ret>
									</loop>
									<rex "on:\P=.+" $cells2>
									<fetch localhost/index.html $ret>
									<urltext>
									<$lastpostdate=$ret>
									<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
								</if>
							</loop>
						</if>
					</loop>
					<rex "</table>\P=.+" $coldata>
					<$htmlbody=$ret>
					<fetch localhost/index.html $htmlbody>
					<urltext>
					<$postbody=$ret>
					<removequotes bodydata=$postbody bodynoquotes=$&bodynoquotes>
				</if>
			</loop>
		</loop>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $FIRSTPAGE eq 1 and $rexloop eq 0>
				<$threadstarter=1>
			<else>
				<$threadstarter=0>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$bodynoquotes
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=4>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>

<a name=parsemethod5 private textdata>
method5
	<domain_blocker callingmethod='5' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>
	<rex row '<table=!<table+>>action\=profile=!action\=profile+' $textdata>
		<$postdata=$ret>
		<$rexloop=$loop>
		<clearpostrecbuffer>
		<getanchor data=$postdata>
		<$posttable=>
		<gettables data=$postdata tables=$&tables>
		<loop max=1 $tables>
			<$posttable=$tables>
		</loop>
		<getrows data=$posttable rows=$&rows>
		<loop max=1 $rows>
			<getcells data=$rows cells=$&cells>
			<loop $cells>
				<$cellnext=$next>
				<rex ">\P=.+" $cells>
				<$coldata=$ret>
				<if $cellnext eq 1>
					<getlinktext hreftext="action\=profile" data=$coldata linktext=$&linktext>
					<$author=$linktext>
					<fetch $THREADURL $coldata>
					<getauthorurl>
				<elseif $cellnext eq 2>
					<getrows data=$coldata rows=$&rows2>
					<loop $rows2>
						<if $next eq 1>
							<getcells data=$rows2 cells=$&cells2>
							<loop $cells2>
								<if $next eq 2>
									<rex '<div=!<div+>>id\="subject_=.+' $cells2>
									<getdivs data=$ret divs=$&divs>
									<loop max=1 $divs>
										<rex ">\P=.+" $divs>
										<fetch localhost/index.html $ret>
										<urltext>
										<$posttitle=$ret>
									</loop>
									<rex "on:\P=.+" $cells2>
									<fetch localhost/index.html $ret>
									<urltext>
									<$lastpostdate=$ret>
									<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
								</if>
							</loop>
						</if>
					</loop>
					<rex "</table>\P=.+" $coldata>
					<$htmlbody=$ret>
					<fetch localhost/index.html $htmlbody>
					<urltext>
					<$postbody=$ret>
					<removequotes bodydata=$postbody bodynoquotes=$&bodynoquotes>
				</if>
			</loop>
			<if $lastpostdate eq "">
				<rex '<table=!<table+>>class\="postdate"=.+' $posttable>
				<gettables data=$ret tables=$&tables>
				<loop max=1 $tables>
					<getcells data=$tables cells=$&cells2>
					<loop $cells2>
						<if $next eq 2>
							<rex "on:\P=.+" $cells2>
							<fetch localhost/index.html $ret>
							<urltext>
							<$lastpostdate=$ret>
							<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
						</if>
					</loop>
				</loop>
			</if>
			<if $postbody eq "">
				<rex '>><div\spaceclass\="post">=.+' $posttable>
				<getdivs data=$ret divs=$&divs>
				<loop max=1 $divs>
					<rex ">\P=.+" $divs>
					<$htmlbody=$ret>
					<fetch localhost/index.html $htmlbody>
					<urltext>
					<$postbody=$ret>
					<removequotes bodydata=$postbody bodynoquotes=$&bodynoquotes>
				</loop>
			</if>
		</loop>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $FIRSTPAGE eq 1 and $rexloop eq 0>
				<$threadstarter=1>
			<else>
				<$threadstarter=0>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$bodynoquotes
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter  signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=5>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>

<a name=parsemethod7 private textdata>
method7
	<domain_blocker callingmethod='7' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>
	<rex row '>><a\spacename\="msg=!<a\spacename\="msg+' $textdata>
		<$postdata=$ret>
		<$rexloop=$loop>
		<clearpostrecbuffer>
		<$posttable=>
		<$postbodytable=>
		<getanchor data=$postdata>
		<$posttable=>
		<rex max=1 '<table\P=.+' $postdata></rex>
		<gettables data=$ret tables=$&tables>
		<loop max=3 $tables>
			<!-- if it's a first post on the page, read 2nd and 3rd table; if not a first post, read 1st and 2nd table -->
			<if $rexloop eq 1><!-- second rex match is actually the (first) valid post -->
				<if $next eq 2>
					<$posttable=$tables>
				<elseif $next eq 3>
					<$postbodytable=$tables>
				</if>
			<else>
				<if $next eq 1>
					<$posttable=$tables>
				<elseif $next eq 2>
					<$postbodytable=$tables>
				</if>
			</if>
		</loop>
		<rex row '<a\space=!<a\space+>>action\=profile=!</a>+</a>' $postdata>
			<$linkdata=$ret>
			<fetch $THREADURL $linkdata>
			<urltext>
			<$author=$ret>
			<trimmer inout=$&author>
			<if $author ne "">
				<urllinks>
				<loop max=1 $ret>
					<$authorurl=$ret>
				</loop>
				<break>
			</if>
		</rex>
		<rex '>><span\spacestyle\="font\-size:\spacex\-small;=.+' $posttable>
		<getspans data=$ret spans=$&spans>
		<loop max=1 $spans>
			<rex ">\P=.+" $spans>
			<fetch localhost/index.html $ret>
			<urltext>
			<$lastpostdate=$ret>
			<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
		</loop>
		<getcells data=$postbodytable cells=$&cells>
		<loop max=2 $cells>
			<if $next eq 2>
				<rex ">\P=.+" $cells>
				<$htmlbody=$ret>
				<fetch localhost/index.html $htmlbody>
				<urltext>
				<$postbody=$ret>
				<removequotes bodydata=$postbody bodynoquotes=$&bodynoquotes>
			</if>
		</loop>
		<rex '>><div\spaceclass\="signature"=.+' $postdata>
		<getdivs data=$ret divs=$&divs>
		<loop max=1 $divs>
			<rex ">\P=.+" $divs>
			<$signature=$ret>
		</loop>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $FIRSTPAGE eq 1 and $rexloop eq 1>
				<$threadstarter=1>
			<else>
				<$threadstarter=0>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$bodynoquotes
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter  signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=7>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>

<a name=parsemethod8 private textdata>
method8
	<domain_blocker callingmethod='8' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>
	<rex row '>><a\spacename\="msg=!<a\spacename\="msg+' $textdata>
		<$postdata=$ret>
		<$rexloop=$loop>
		<clearpostrecbuffer>
		<getanchor data=$postdata>
		<$postrow1=>
		<$postrow2=>
		<!-- pools in 'darkreign40k.com' first post was not working-->
		<rex max=1 '>>_poll\.gif"' $postdata></rex>
		<if $ret neq ''  and $SITEDOMAIN eq 'darkreign40k.com' and  $rexloop eq 0 >
			<rex row '>><td\spaceclass\="windowbg=!<td\spaceclass\="windowbg+' $postdata>
				<$temp=$ret>
			</rex>
		<else>
			<rex max=1 '>><td\spaceclass\="windowbg=.+' $postdata></rex>
			<$temp=$ret>
		</if>
		<getrows data=$temp rows=$&rows>
		<loop max=2 $rows>
			<if $next eq 1>
				<$postrow1=$rows>
			<elseif $next eq 2>
				<$postrow2=$rows>
			</if>
		</loop>
		<getcells data=$postrow1 cells=$&cells>
		<loop $cells>
			<$cellnext=$next>
			<if $cellnext eq 1>
				<!-- AUTHOR, URL & LOC -->
				<rex max=1 '<a\space=!<a\space+>>action\=profile=!</a>+</a>=' $cells></rex>
				<fetch $THREADURL $ret>
				<urllinks>
				<$authorurl=$ret>
				<urltext>
				<$author=$ret>
				<trimmer inout=$&author>
				<rex max=1 '>>Location:\P=!<+' $cells></rex>
				<$authorloc=$ret>
				<trimmer inout=$&authorloc>
				<rex max=1 '>>Age: \P=!<+' $cells></rex>
				<$authorage=$ret>
				<trimmer inout=$&authorage>
			<elseif $cellnext eq 2>
				<getdivs data=$cells divs=$&divs>
				<loop max=2 $divs>
					<if $next eq 1>
						<gettagtextorhtml tag=$divs istext=1 textret=$&posttitle>
					<elseif $next eq 2>
						<gettagtextorhtml tag=$divs istext=1 textret=$&lastpostdate>
						<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
					</if>
				</loop>
				<!-- BODY - just in case, extract the specific div, matched by class -->
				<rex '>><div\spaceclass\="post"=.+' $cells>
				<getdivs data=$ret divs=$&bodydivs>
				<loop max=1 $bodydivs>
					<gettagtextorhtml tag=$bodydivs istext=0 textret=$&htmlbody>
					<gettagtextorhtml tag=$bodydivs istext=1 textret=$&postbody>
				</loop>
			</if>
		</loop>
		<!-- SIGNATURE -->
		<rex '>><div\spaceclass\="signature"=.+' $postrow2>
		<getdivs data=$ret divs=$&sigdivs>
		<loop max=1 $sigdivs>
			<gettagtextorhtml tag=$sigdivs istext=0 textret=$&signature>
		</loop>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $FIRSTPAGE eq 1 and $rexloop eq 0>
				<$threadstarter=1>
			<else>
				<$threadstarter=0>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$postbody
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=8>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>

<a name=parsemethod9 private textdata>
method9
	<domain_blocker callingmethod='9' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>
	<rex row '>><a\spacename\="msg=!<a\spacename\="msg+' $textdata>
		<$reversecount=$loop>
	</rex>
	<rex row '>><a\spacename\="msg=!<a\spacename\="msg+' $textdata>
		<$postdata=$ret>
		<$rexloop=$loop>
		<clearpostrecbuffer>
		<$postheader=>
		<$postcontent=>
		<$sigcontent=>
		<getanchor data=$postdata>
		<$posttable=>
		<rex max=1 '<div\spaceclass\="floatleft=.+' $postdata></rex>
		<getdivs data=$ret divs=$&divs>
		<loop max=3 $divs>
			<if $next eq 1>
				<$postheader=$divs>
			<elseif $next eq 2>
				<$postcontent=$divs>
			<elseif $next eq 3>
				<$sigcontent=$divs>
			</if>
		</loop>
<!--TABLE 1:
<send $postheader>

##################################################
TABLE 2:
<fmt "%!H\r\n"  $postcontent>

##################################################-->
		<rex row '<a\space=!<a\space+>>action\=profile=!</a>+</a>' $postheader>
			<$linkdata=$ret>
			<fetch $THREADURL $linkdata>
			<urltext>
			<$author=$ret>
			<trimmer inout=$&author>
			<if $author ne "">
				<urllinks>
				<loop max=1 $ret>
					<$authorurl=$ret>
				</loop>
				<break>
			</if>
		</rex>
		<if $author eq ''>
			<rex max=1 '>><h4>\P=!</h4>+' $postheader></rex>
			<$author=$ret><!-- no active link - no author url -->
		</if>
		<rex max=1 '>>/Female\.gif"' $postheader></rex>
		<if $ret eq ''>
			<rex max=1 '>>/Male\.gif"' $postheader></rex>
			<if $ret neq ''>
				<$gender='M'>
			</if>
		<else>
			<$gender='F'>
		</if>
		<rex '>><h5 id\="subject_=!</h5>+</h5>=' $postcontent>
		<fetch localhost/index.html $ret>
		<urltext>
		<$posttitle=$ret>
		<trimmer inout=$&posttitle>
		<!--LAST POST DATE-->
		<$daterex ='\space*>>on:</strong>\P=!<+' '\space*>>à¹€à¸¡à¸·à¹ˆà¸­:</strong>\P=!<+'
			'\space*>>on:</strong>\P=\space*<b>=!</b>+</b>=!<+' '>>&#171;=\space*<strong>=!</strong>*</strong>\P=!&#187;+\F&#187;='>
		<rex $daterex $postcontent>
		<sandr '&#187' '' $ret>
		<sandr '<=/?b>=' '' $ret>
		<$lastpostdate=$ret>
		<trimmer inout=$&lastpostdate>
		<!-- making sure we get the single-value from the rex... because the multi-patterns may match 2 different results -->
		<local tempdate1>
		<$tempdate1=$lastpostdate>
		<loop $lastpostdate>
			<if $lastpostdate ne ''>
				<$tempdate1=$lastpostdate>
				<break>
			</if>
		</loop>
		<$lastpostdate=$tempdate1>
		<!---------------------->
		<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
		<getdivs data=$postcontent divs=$&divs>
		<loop max=2 $divs>
			<if $next eq 2>
				<gettagtextorhtml tag=$divs istext=0 textret=$&htmlbody>
				<gettagtextorhtml tag=$divs istext=1 textret=$&postbody>
				<removequotes bodydata=$postbody bodynoquotes=$&bodynoquotes>
			</if>
		</loop>
		<rex '>><div\spaceclass\="signature"=.+' $sigcontent>
		<getdivs data=$ret divs=$&sigdivs>
		<loop max=1 $sigdivs>
			<gettagtextorhtml tag=$sigdivs istext=0 textret=$&signature>
		</loop>
		<!-- AVATAR -->
		<$avatarrex='<img=!<img+>>class\="avatar"=!>+>=' >
		<rex max=1 $avatarrex $postdata></rex>
		<getimagesrc htmlsrc=$ret domain=$THREADURL srcret=$&avatarurl>
		<!-- AUTHOR LOC -->
		<$locrex='>><li>location:\P=!<+'>
		<rex $locrex $postdata>
		<fetch localhost/index.html $ret>
		<urltext>
		<$authorloc=$ret>
		<trimmer inout=$&authorloc>
		<!-- SITEPOSTID - using anchor value by default -->
		<loop max=1 $anchor><$sitepostid=$anchor></loop>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $HASUNIQUESITEPOSTID eq 1 and $sitepostid eq '' >
INVALID POST - SITEPOSTID REQUIRED
		<elseif $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<$threadstarter=0>
			<if $SITEDOMAIN eq 'www.smokey-services.eu'>
				<!-- code for posts displayed in reverse order -->
				<if $reversecount eq $rexloop and $FIRSTPAGE eq 1>
					<$threadstarter=1>
				</if>
			<elseif ($SITEDOMAIN eq 'forum.domohozyaika.com' or $SITEDOMAIN eq 'smf.rcw.ms')>
				<if $FIRSTPAGE eq 1 and $rexloop eq 0>
					<$threadstarter=1>
				</if>
			<else>
				<if $FIRSTPAGE eq 1 and $rexloop eq 1>
					<$threadstarter=1>
				</if>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$bodynoquotes
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=9>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>

<a name=parsemethod10 private textdata>
method10
	<domain_blocker callingmethod='10' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>
	<$mainrex='>><a id\="msg=!<a id\="msg+'>
	<if $SITEDOMAIN eq 'www.satellite-audiovideo.com'>
		<$mainrex='>><a name\="msg=!<a name\="msg+'>
	</if>
	<!-- posts displayed in reverse order for this site, so we have to determine how many posts are on page before parsing -->
	<rex row $mainrex $textdata>
		<$reversecount=$loop>
	</rex>
	<rex row $mainrex $textdata>
		<sandr '>><a id\="lastPost">=.+' '' $ret>
		<$postdata=$ret>
		<$rexloop=$loop>
		<clearpostrecbuffer>
		<!-- ANCHOR -->
		<rex '>><a id\="\P=msg=\digit+' $postdata>
		<$anchor=$ret>
		<!-- cleanup, especially the first post -->
		<if $SITEDOMAIN eq 'www.mvisioneros.com'>
			<sandr '<=!<+>>class\="ThankOMatic=.+' '' $postdata>
			<$postdata = $ret>
		<elseif $SITEDOMAIN eq 'www.allaboutdogfood.co.uk'>
			<sandr '>><div class\="like=.+' '' $postdata>
			<$postdata = $ret>
		</if>
		<if $SITEDOMAIN eq 'forums.tmonews.com' or $SITEDOMAIN eq 'www.chevyavalancheclub.com' or $SITEDOMAIN eq 'www.fertilityfriends.co.uk'
			or $SITEDOMAIN eq 'www.kneeguru.co.uk' or $SITEDOMAIN eq 'www.babywhispererforums.com' or $SITEDOMAIN eq 'www.gendersupport.org' or $SITEDOMAIN eq 'www.anxietyzone.com' or $SITEDOMAIN eq 'www.susans.org'
			or $SITEDOMAIN eq 'clubzafira.com' or $SITEDOMAIN eq 'talk.uk-yankee.com'>
			<sandr '.+<div=!<...+>>\Fclass\="post_wrapper"=' '' $postdata>
		<else>
			<sandr '.+\F<div=!<div+>>action\=profile=' '' $postdata>
		</if>
		<getdivs data=$ret divs=$&divs>
		<$leftdiv=>
		<$rightdiv=>
		<loop max=2 $divs>
			<if $next eq 1>
				<$leftdiv=$divs>
			<else>
				<$rightdiv=$divs>

			</if>
		</loop>
		<!-- AUTHOR & URL-->
		<rex max=1 '<a =!<a +>>action\=profile=!</a>+</a>=' $leftdiv></rex>
		<fetch $THREADURL $ret>
		<urllinks>
		<$authorurl=$ret>
		<urltext>
		<$author=$ret>
		<trimmer inout=$&author>
		<if $author eq '' and ($SITEDOMAIN eq 'www.fertilityfriends.co.uk' or $SITEDOMAIN eq 'www.kneeguru.co.uk'
			or $SITEDOMAIN eq 'www.babywhispererforums.com' or $SITEDOMAIN eq 'www.gendersupport.org' or $SITEDOMAIN eq 'www.anxietyzone.com'
			or $SITEDOMAIN eq 'www.susans.org' or $SITEDOMAIN eq 'clubzafira.com' or $SITEDOMAIN eq 'talk.uk-yankee.com')>
			<rex max=1 '<div=!<...+>>class\="poster"=!</h4>+</h4>=' $postdata></rex>
			<fetch localhost/html $ret>
			<urltext>
			<$author=$ret>
			<trimmer inout=$&author>
		</if>
		<if $SITEDOMAIN eq 'forums.tmonews.com'>
			<rex '>><h4 class\="postauthor"=!</h4>+</h4>=' $postdata>
			<$temp = $ret>
			<rex max=1 '<a =!<a +>>/profile/=!</a>+</a>=' $temp></rex>
			<fetch $THREADURL $ret>
			<urllinks>
			<$authorurl=$ret>
			<urltext>
			<$author=$ret>
			<trimmer inout=$&author>
			<if $author eq ''>
				<fetch localhost/index.html $temp>
				<urltext>
				<$author=$ret>
				<trimmer inout=$&author>
			</if>
		</if>
		<!-- AVATAR -->
		<$avatarrex='<img=!<img+>>custom_avatars/avatar=!>+>=' '<img=!<img+>>class\="avatar"=!>+>='>
		<!-- these sites have img.avatar nested in a li.avatar -->
		<if $SITEDOMAIN eq 'www.redandwhitekop.com' or $SITEDOMAIN eq 'www.svenskamord.se'
			or $SITEDOMAIN eq 'www.candymag.com' or $SITEDOMAIN eq 'www.esaonline.co.za' or $SITEDOMAIN eq 'www.wildfisher.co.uk'
			or $SITEDOMAIN eq 'forums.tmonews.com' or $SITEDOMAIN eq 'www.timesharetalk.co.uk' or $SITEDOMAIN eq 'www.anxietyzone.com'
			or $SITEDOMAIN eq 'www.gendersupport.org' or $SITEDOMAIN eq 'www.susans.org'>
			<$avatarrex='<li=\space+>>class\="avatar"=!<img\space\P+<img\space=!>*>='>
		</if>
		<$reject='Themes/rawkv2/images/star\.gif' '/images/noavatar.png' '/avatars/noavatar\.gif' '/avatars/noavatar.png'>
		<rex max=1 $avatarrex $leftdiv></rex>
		<getimagesrc htmlsrc=$ret domain=$THREADURL srcret=$&avatarurl reject=$reject>
		<!-- TITLE -->
		<rex '>><h\digit id\="subject_=!</h+' $rightdiv>
		<fetch localhost/index.html $ret>
		<urltext>
		<$posttitle=$ret>
		<trimmer inout=$&posttitle>
		<!-- BODY -->
		<rex '>><div\spaceclass\="post"=.+' $rightdiv>
		<getdivs data=$ret divs=$&bodydiv>
		<loop max=1 $bodydiv>
			<if $SITEDOMAIN eq 'www.chevyavalancheclub.com'>
				<rex '<div=!<...+>>class\="attachments =.+' $rightdiv>
				<getdivs data=$ret divs=$&temp>
				<loop max=1 $temp>
					<gettagtextorhtml tag=$temp istext=0 textret=$&temp2>
					<sum '%s' $bodydiv $temp2>
					<$bodydiv = $ret>
				</loop>
			</if>
			<gettagtextorhtml tag=$bodydiv istext=0 textret=$&htmlbody>
			<gettagtextorhtml tag=$bodydiv istext=1 textret=$&postbody>
		</loop>
		<!-- DATE -->
		<$daterex='on:</strong>\P=!<+' '>>\spaceskrivet:</strong>\P=!<+' '>> le:</strong>\P=!<+'
			' am:</strong>\P=!</div+'>
		<if $SITEDOMAIN eq 'www.blackberry.gen.tr' or $SITEDOMAIN eq 'www.vzlafta.com' or $SITEDOMAIN eq 'keskustelu.diabetes.fi'
				or $SITEDOMAIN eq 'keskustelukanava.agronet.fi' or $SITEDOMAIN eq 'forvol.ru' or $SITEDOMAIN eq 'kroufr.ru'
				or $SITEDOMAIN eq 'www.mandrivabrasil.org'>
			<$daterex='>>&#171;\P=!&#187;+'>
		</if>
		<rex max=1 $daterex $rightdiv></rex>
		<sandr ' &agrave; ' ' ' $ret>
		<sandr ' um ' ' ' $ret>
		<sandr 'kello ' ' ' $ret>
		<sandr ' Ð² ' ' ' $ret>
		<sandr ' às ' ' ' $ret>
		<if $LANG eq "Portuguese">
			<sandr ' de ' ' ' $ret>
		</if>
		<if $SITEDOMAIN eq 'www.mvisioneros.com' or $SITEDOMAIN eq 'www.lacomunateleco.com'
				or $SITEDOMAIN eq 'forums.tmonews.com'>
			<rex max=1 '>><div class\="keyinfo"=!<div class\="smalltext"\P+!</div>+</div>=' $rightdiv></rex>
			<sandr '>><strong>=!</strong>+</strong>=' '' $ret>
		</if>
		<if $SITEDOMAIN eq 'clubzafira.com'>
			<rex max=1 '>><div class\="keyinfo"=!<div class\="smalltext"\P+!</div>+</div>=' $rightdiv></rex>
			<sandr '.+>>en:' '' $ret>
		</if>		
		<fetch localhost/index.html $ret>
		<urltext>
		<$lastpostdate=$ret>
		<trimmer inout=$&lastpostdate>
		<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
		<if $lastpostdate eq ''>
			<rex max=1 '<div=!<div+>>on:</strong>=!</div>+</div>' $rightdiv></rex>
			<fetch localhost/index.html $ret>
			<urltext>
			<$lastpostdate=$ret>
			<trimmer inout=$&lastpostdate>
			<checkdate lang=$LANG isdateonly=0 indate=$&lastpostdate indateid=$&TIMEFORMATID>
		</if>
		<!-- SIGNATURE -->
		<rex max=1 '>><div class\="signature"=.+' $postdata></rex>
		<getdivs data=$ret divs=$&divs>
		<loop max=1 $divs>
			<rex max=1 '>>>\P=.+' $divs></rex>
			<$signature=$ret>
		</loop>
		<!-- AUTHOR SEX -->
		<$femalerex='>>female\.gif"' '>>female\.png"'>
		<$malerex='>>male\.gif"' '>>male\.png"'>
		<rex max=1 $femalerex $leftdiv></rex>
		<if $ret eq ''>
			<rex max=1 $malerex $leftdiv></rex>
			<if $ret neq ''>
				<$gender='M'>
			</if>
		<else>
			<$gender='F'>
		</if>
		<!-- AUTHOR LOC -->
		<$locrex='>>>Yer:\P=!<+' '>>>Location:\P=!<+'>
		<if $SITEDOMAIN eq 'www.babywhispererforums.com'>
			<$locrex='<b>=>>Location:</b>\P=!<br+\F<br='>
		</if>
		<rex $locrex $leftdiv>
		<fetch localhost/index.html $ret>
		<urltext>
		<$authorloc=$ret>
		<trimmer inout=$&authorloc>
		<if $authorloc eq ''>
			<!-- take text from image -->
			<$locrex='>>>Country:=!>+>='>
			<rex $locrex $leftdiv>
			<rex max=1 '>>title\="\P=!"+' $ret></rex>
			<fetch localhost/index.html $ret>
			<urltext>
			<$authorloc=$ret>
			<trimmer inout=$&authorloc>
		</if>
		<if $REGISTRATIONDATEFORMATID ne '' AND $REGISTRATIONDATEFORMATID ne '0'>
R1
			<$authorregrex='>>Registo:\P=!<+\F<' '>>>Join Date:\P=!<+' '>><b>Joined:</b>\P=!<+'>
			<if $SITEDOMAIN eq 'talk.uk-yankee.com'>
				<$authorregrex='>>>Joined:\P=!<+'>
			</if>			
			<rex $authorregrex $postdata>
			<fetch localhost/index.html $ret>
			<urltext>
			<$registered=$ret>
			<trimmer inout=$&registered>
			<fixregdate passdate=$&registered>
			<checkdate lang=$LANG isdateonly=1 indate=$&registered indateid=$REGISTRATIONDATEFORMATID registereddate=1>
		</if>
		<!-- SITEPOSTID - using anchor value by default -->
		<loop max=1 $anchor><$sitepostid=$anchor></loop>
		<if $anchor eq '' or $lastpostdate eq '' or $author eq '' or $htmlbody eq ''>
			<$invalidfound=1>
		<else>
			<$ALLPOSTSCNT=($ALLPOSTSCNT+1)>
		</if>
<if $TEST_DEBUG_LOCAL eq 1>
	<debug>
</if>
		<if $HASUNIQUESITEPOSTID eq 1 and $sitepostid eq '' >
INVALID POST - SITEPOSTID REQUIRED
		<elseif $invalidfound eq 0 and $lastpostdate gte $FORUMLASTCRAWLED and $lastpostdate gte $MAXDAYS>
			<if $SITEDOMAIN eq 'www.purrsinourhearts.co.uk'>
				<!-- code for posts displayed in reverse order -->
				<if $reversecount eq $rexloop and $stopperflag ne 1>
					<$threadstarter=1>
					<$stopperflag=1>
				<else>
					<$threadstarter=0>
				</if>
			<else>
				<if $FIRSTPAGE eq 1 and $rexloop eq 0>
					<$threadstarter=1>
				<else>
					<$threadstarter=0>
				</if>
			</if>
			<insertpost threadtitle=$THREADTITLE postdate=$lastpostdate threadid=$INSERTEDTHREADID
				forumid=$FORUMID siteid=$SITEID	urlthreadid=$URLTHREADID
				threadurl=$THREADURL basethreadurl=$BASETHREADURL posttitle=$posttitle postbody=$postbody bodynoquotes=$postbody
				linktosite=$LINKTOSITE hotlink=$HOTLINK
				sitealias=$SITEALIAS siteurl=$SITEURL author=$author authorurl=$authorurl
				anchor=$anchor authorloc=$authorloc bbstype=$BBSTYPE bbsver=$BBSVER
				lang=$LANG cacheposts=$CACHEPOSTS forumname=$FORUMNAME
				forumurl=$FORUMURL sitetitle=$SITETITLE htmlbody=$htmlbody charset=$CHARSET
				age=$authorage sex=$gender gmt=$GMT adult=$ADULT country=$COUNTRY
				threadstarter=$threadstarter signature=$signature avatarurl=$avatarurl
				hasuniquesitepostid=$HASUNIQUESITEPOSTID feedsources=$FEEDSOURCES registered=$registered sitepostid=$sitepostid
				sitetopic=$SITETOPIC forumtopics=$FORUMTOPICS forumcategories=$FORUMCATEGORIES
				providerid=$PROVIDERID
				inserted=$&inserted invalidfound=$&invalidfound>
******************************************
			<if $inserted eq 1>
				<$PARSEMETHOD=10>
				<$POSTSCNT=($POSTSCNT+1)>
			<else>
				<$DUPSFOUND=1>
			</if>
			<$POSTSPARSED=1>
		<elseif $lastpostdate lt $FORUMLASTCRAWLED>
			<if $invalidfound eq 0>
				<$POSTSPARSED=1>
			</if>
		</if>
-----------------------------------
	</rex>
</a>

<a name=parsemethod11 private textdata>
method11
	<domain_blocker callingmethod='11' myreturn=$&stopproc>
	<if $stopproc eq 1>
		<return>
	</if>	
	<$mainrex='>><a id\="msg=!<a id\="msg+'>
	<if $SITEDOMAIN eq 'automir.dp.ua' or $SITEDOMAIN eq 'www.cocktaildreams.de'>
		<$mainrex='>><a name\="msg=!<a name\="msg+'>
	<elseif $SITEDOMAIN eq 'www.go-windows.de'>
		<$mainrex='>><a\X0Aid\="msg=!<a\X0Aid\="msg+'>
	</if>
	<rex row $mainrex $textdata>
		<$postdata=$ret>
		<$rexloop=$loop>
		<clearpostrecbuffer>
		<!-- cleanup -->
		<sandr '>><span id\="gpbp_score_=!</span>+' '' $postdata>
		<$postdata=$ret>
		<!-- ANCHOR -->
		<rex '>><a id\="\P=msg=\digit+' $postdata>
		<if $ret eq ''>
			<rex '>><a name\="\P=msg=\digit+' $postdata>
		</if>
		<if $SITEDOMAIN eq 'www.go-windows.de'>
			<rex '>><a\X0Aid\="\P=msg=\digit+' $postdata>
		</if>
		<$anchor=$ret>
		<!-- AUTHOR & URL-->
A1
		<$authorrex='<a =!<a +>>action\=profile=!</a>+</a>=' '<a =!<a +>>/profile/=!</a>+</a>=' '<a\X0A=!<a\X0A+>>/profile/=!</a>+</a>=' '<a=!<.+>>/profiel/=!</a>+</a>='>
		<if $SITEDOMAIN eq 'forum.alternatifim.com'>
			<$authorrex='<li=!<..+>>class\="postprofilvrh=!</li>+</li>='>
		<elseif $SITEDOMAIN eq 'www.cocktaildreams.de'>
			<$authorrex='<a=!<a+>>title\="Profil=!</a>+</a>='>
		</if>
		<rex max=1 $authorrex $postdata></rex>
		<fetch $THREADURL $ret>
		<urllinks>
		<$authorurl=$ret>
		<urltext>
		<$author=$ret>
		<trimmer inout=$&author>
		<if $author eq ''>
A2
			<rex max=1 '>>class\="poster">\P=.+' $postdata></rex>
			<if $SITEDOMAIN eq 'www.go-windows.de' or $SITEDOMAIN eq 'forum.leben-mit-ms.de'
				or $SITEDOMAIN eq 'foro.tiempo.com' or $SITEDOMAIN eq 'www.portalhacker.net'
				or $SITEDOMAIN eq 'foro.diariocritico.com'  or $SITEDOMAIN eq 'www.basso.fi'
				or $SITEDOMAIN eq 'expressionwebforum.com' or $SITEDOMAIN eq 'forum.freenews.fr'
				or $SITEDOMAIN eq 'www.svetecigaret.cz' or $SITEDOMAIN eq 'forum.vitalia.cz'
				or $SITEDOMAIN eq 'forum.podnikatel.cz' or $SITEDOMAIN eq 'brzytwa.org' or $SITEDOMAIN eq 'forum.ostrow24.tv'
				or $SITEDOMAIN eq 'forum.imoneybank.ru' or $SITEDOMAIN eq 'shopoforum.ru'
				or $SITEDOMAIN eq 'www.hobbyconsolas.com' or $SITEDOMAIN eq 'www.myswag.org'
				or $SITEDOMAIN eq 'forza-club.org.ua' or $SITEDOMAIN eq 'www.nuggetoutdoors.com.au' 
				or $SITEDOMAIN eq 'www.benzobuddies.org' or $SITEDOMAIN eq 'forum.sekswoordenboek.nl'
				or $SITEDOMAIN eq 'farmnest.com' or $SITEDOMAIN eq 'forococheselectricos.com' 
				or $SITEDOMAIN eq 'atarnotes.com' or $SITEDOMAIN eq 'www.jeepowners.club'
				or $SITEDOMAIN eq 'kvartiradin.ru' or $SITEDOMAIN eq 'www.forum.e-mebel.net'
				or $SITEDOMAIN eq 'forum.girlscene.nl'>
				<rex max=1 '>><h4=!</h4>+' $ret></rex>
			<elseif $SITEDOMAIN eq 'www.toyota-club.com.ua'>
				<rex max=1 '>><h4=!<+' $postdata></rex>
			</if>
			<fetch localhost/index.html $ret>
			<urltext>
			<$author=$ret>
			<trimmer inout=$&author>
		</if>
		<if $author eq ''>
A3
