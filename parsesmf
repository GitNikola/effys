			<rex max=1 '<span=!<....+>>class\="name"=!<span>+<span>=' $postdata></rex>
			<rex max=1 '>><a=!</a>+</a>=' $ret></rex>
			<fetch $THREADURL $ret>
			<urllinks>
			<$authorurl=$ret>
			<urltext>
			<$author=$ret>
			<trimmer inout=$&author>
		</if>
		<!-- AVATAR -->
		<$avatarrex='<img=!<img+>>class\="avatar"=!>+>=' >
		<if $SITEDOMAIN eq 'expressionwebforum.com' or $SITEDOMAIN eq 'www.captiva-club.com'>
			<!-- above rex can make problems, since there could be class avatar inside <LI TAG-->
			<$avatarrex='<img=!<...+>>class\="avatar"=!>+>=' >
		<elseif $SITEDOMAIN eq 'www.forofocus.com'>
			<!-- these sites have img.avatar nested in a li.avatar -->
			<$avatarrex='<li=\space+>>class\="avatar"=!<img\space\P+<img\space=!>*>='>
		<elseif $SITEDOMAIN eq 'rebenok.by' or $SITEDOMAIN eq 'dodge-club.org.ua' or $SITEDOMAIN eq 'buy-club.ru'
				or $SITEDOMAIN eq 'forum.b-m-w.ru' or $SITEDOMAIN eq '38mama.ru'>
			<$avatarrex='>><li class\="avatar=!</li>+'>
		<elseif $SITEDOMAIN eq 'www.go-windows.de'>
			<$avatarrex='>><li\X0Aclass\="avatar=!</li>+'>
		</if>
		<$avreject='/avatars/blank\.gif'>
		<rex max=1 $avatarrex $postdata></rex>
		<getimagesrc htmlsrc=$ret domain=$THREADURL srcret=$&avatarurl reject=$avreject>
		<!-- TITLE -->
		<!--#########################
		<send $postdata>

		#########################-->
		<if $SITEDOMAIN neq 'www.basso.fi'>
			<rex '>><h\digit id\="subject_=!</h+' $postdata>
			<if $SITEDOMAIN eq 'automir.dp.ua'>
				<rex max=1 '>><b class\="inthemesubject=!</b>+' $postdata></rex>
			<elseif $SITEDOMAIN eq 'www.cocktaildreams.de'>
				<rex max=1 '<div=!<div+>>id\="subject_=!</div>+' $postdata></rex>
			</if>
			<fetch localhost/index.html $ret>
			<urltext>
			<$posttitle=$ret>
			<trimmer inout=$&posttitle>
		</if>
		<!-- BODY -->
		<$bodyrex='>><div\spaceclass\="post"=.+' '>><div\spaceclass\="post guest"=.+'>
		<if $SITEDOMAIN eq 'rebenok.by'>
			<$bodyrex='>><div class\="inner" id\="msg_=.+'>
		</if>
		<rex $bodyrex $postdata>
		<if $SITEDOMAIN eq 'www.go-windows.de'>
			<sandr '>><div\X0Aclass\="titlebg=!</div>+</div>=' '' $ret>
			<sandr '>><div\X0Astyle\="margin:5px;=!</div>+</div>=' '' $ret>
		</if>
		<getdivs data=$ret divs=$&bodydiv>
		<loop max=1 $bodydiv>
			<if $SITEDOMAIN eq 'foros.elle.es' or $SITEDOMAIN eq 'www.jeepowners.club'>
				<rex '<div=!<...+>>class\="attachments =.+' $postdata>
				<getdivs data=$ret divs=$&temp>
				<loop max=1 $temp>
					<gettagtextorhtml tag=$temp istext=0 textret=$&temp2>
					<sum '%s' $bodydiv $temp2>
					<$bodydiv = $ret>
				</loop>
			</if>
			<add_cleanup inouthtml=$&bodydiv>
			<gettagtextorhtml tag=$bodydiv istext=0 textret=$&htmlbody>
			<gettagtextorhtml tag=$bodydiv istext=1 textret=$&postbody>
		</loop>
		<!-- DATE -->
		<$daterex= "on:</strong>\P=!<+"  "en:</strong>\P=!</div+" "am:</strong>\P=!</div+" "il:</strong>\P=!</div+">
		<if $SITEDOMAIN eq 'www.teplos.net' or $SITEDOMAIN eq 'automir.dp.ua' or $SITEDOMAIN eq 'www.cocktaildreams.de'>
			<$daterex='>>&#171;\P=!&#187;+'>
		</if>
		<rex max=1 $daterex $postdata></rex>
		<if $ret eq '' or $SITEDOMAIN eq 'foros.elle.es' or $SITEDOMAIN eq 'www.basso.fi' or $SITEDOMAIN eq 'foro.tuning-on.com'
			or $SITEDOMAIN eq 'hommaforum.org' or $SITEDOMAIN eq 'www.cyclingtopics.com' or $SITEDOMAIN eq 'forum.mesec.cz'
			or $SITEDOMAIN eq 'forum.freenews.fr' or $SITEDOMAIN eq 'www.svetecigaret.cz' or $SITEDOMAIN eq 'forum.vitalia.cz'
			or $SITEDOMAIN eq 'forum.podnikatel.cz' or $SITEDOMAIN eq 'brzytwa.org'  or $SITEDOMAIN eq 'forum.ostrow24.tv'
			or $SITEDOMAIN eq 'www.zukiworld.com' or $SITEDOMAIN eq 'forums.poz.com' or $SITEDOMAIN eq 'www.oknamedia.ru'
			or $SITEDOMAIN eq 'forum.imoneybank.ru' or $SITEDOMAIN eq 'www.myswag.org' or $SITEDOMAIN eq 'farmnest.com' 
			or $SITEDOMAIN eq 'www.forum.fordmpv.com' or $SITEDOMAIN eq 'www.birtydastards.com' or $SITEDOMAIN eq 'www.chiefmotorcycleforum.com'
